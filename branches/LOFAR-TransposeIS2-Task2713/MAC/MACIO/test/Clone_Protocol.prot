//
// Protocol definition for testing packing, cloning, etc.
//
autogen definitions protocol;

description = "Protocol for the testing event related stuff";
prefix = "Clone"; // for the signal names
id = "LOFAR::MACIO::F_APL_PROTOCOL";
// specify extra include files
// e.g.
include = '<MACIO/StringVector.h>';
include = '<sys/time.h>';

//
// An "event" has a "signal" and a "dir" (direction)
// and zero or more "param"s.
// "dir" can be one of "IN" or "OUT".
// A "param" has a "name" and a "type".
//
event = {
	signal = Simple;
	dir = IN;
	param = {
		name = "uInt";
		type = "unsigned int";
	};
	param = {
		name = "Int";
		type = "int";
	};
	param = {
		name = "Double";
		type = "double";
	};
	param = {
		name = "Float";
		type = "float";
	};
	param = {
		name = "Secret";
		type = "string";
		nonprintable;
	};
	param = {
		name = "String";
		type = "string";
	};
};

event = {
	signal = Vector;
	dir = IN;
	param = {
		name = "SV";
		type = "MACIO::StringVector";
		userdefined;
		printable;
	};
};

