workCB( string dp, dyn_string dList)
{
int i, j;
dyn_string ds, ds1, ds3, ds4;
dyn_dyn_string ds2;

for ( i= 1; i<= dynlen( dList); i++)
{
  strreplace( dList[i], \" | \", \"|\");
  ds = strsplit( dList[i], \"|\");
  dynAppend(ds1, ds[1]);
  ds2[dynlen(ds2)+1] = makeDynString(ds[2],\"STD_value_not_ok\",\"_3DText\");
  dynAppend(ds3, ds[3]);
}

this.deleteAllLines;
this.appendLines( dynlen( ds1), \"#1\", ds1, \"#2\", ds2, \"#3\", ds3); 


}


