    strreplace(RemoteSystem, \":\", \"\"); // remove : 
 }
 
 
 initHosts();

 systemoverviewConnectUi();

 dpGet($System+\"_DataManager.UseValueArchive:_online.._value\",bHDB);  // Ist das neue Archiv aktiv?

