void showStatisticsPanel(int vt)
{
  dyn_string curSettings;
  string bfName;
  string title;
  unsigned subbandStart;
  unsigned subbandEnd;
  
  if(vt==9999) // show all beamlets. VT 0 is used for station subband statistics
  {
    subbandStart = 0;
    subbandEnd = 127;
    title = \"Statistics - all beamlets\";
  }
  else
  {
    dpGet(\"System1:PAC_VT\"+vt+\"_bfName.:_online.._value\", bfName);
    dpGet(\"System1:PAC_VT\"+vt+\"_\"+bfName+\"_subbandStart.:_online.._value\", subbandStart);
    dpGet(\"System1:PAC_VT\"+vt+\"_\"+bfName+\"_subbandEnd.:_online.._value\", subbandEnd);
    title = \"Statistics - VT\" + vt;
  }
  curSettings = makeDynString(\"$VT:\"+vt,
                              \"$SUBBANDORBEAMLET:Beamlet\",
                              \"$SUBSTART:\"+subbandStart,
                              \"$SUBEND:\"+subbandEnd);
  
  RootPanelOn(\"Station_Statistics.pnl\",
      title,
      curSettings);
}

void setGlobalSettings(int vt, dyn_float df, dyn_string ds)
{
  dpSetWait(\"System1:PAC_VT\"+vt+\"_bfName.:_original.._value\", \"BF\"+vt);
  dpSetWait(\"System1:PAC_VT\"+vt+\"_srgName.:_original.._value\", ds[1]);
  dpSetWait(\"System1:PAC_\"+ds[1]+\"_frequency.:_original.._value\", df[4]);
  dpSetWait(\"System1:PAC_VT\"+vt+\"_BF\"+vt+\"_directionAngle1.:_original.._value\", df[5]);
  dpSetWait(\"System1:PAC_VT\"+vt+\"_BF\"+vt+\"_directionAngle2.:_original.._value\", df[6]);
  dpSetWait(\"System1:PAC_VT\"+vt+\"_BF\"+vt+\"_directionType.:_original.._value\", ds[2]);
  dpSetWait(\"System1:PAC_VT\"+vt+\"_BF\"+vt+\"_subbandStart.:_original.._value\", df[7]);
  dpSetWait(\"System1:PAC_VT\"+vt+\"_BF\"+vt+\"_subbandEnd.:_original.._value\", df[8]);
}

string buildSubbandsString(int vt,unsigned subbandStart,unsigned subbandEnd)
{
  string subbands=\"\";
  int i;

  if(subbandStart < 0)
    subbandStart=0;
  if(subbandStart > 127)
    subbandStart=127;
  if(subbandEnd<subbandStart)
    subbandEnd=subbandStart;

  for(i=subbandStart;i<subbandEnd;i++)
  {
    subbands=subbands+i+\"|\";
  }
  subbands=subbands+i;
  return subbands;
}

