"main()
{
dyn_errClass err;
  string command;
  string status;
  dyn_int selectedLines;
  int scheduleId;
  
  selectedLines = tbl_Schedule.getSelectedLines();
  scheduleId = tbl_Schedule.cellValueRC(selectedLines[1],\"ID\");
  command = \"CANCEL \"+scheduleId;
           
  dpSetWait(\"System1:PAC_LogicalDeviceScheduler_command.:_original.._value\", command);
  err = getLastError();
  if (dynlen(err) > 0)
    errorDialog(err);
  
  delay(1);  
  dpGet(\"System1:PAC_LogicalDeviceScheduler_status.:_online.._value\", status);
  if(status==\"OK\")
  {
    tbl_Schedule.deleteLineN(selectedLines[1]);
  }
}
