void updateAntenna(string dp1, int stateX,
                   string dp2, int stateY,
                   string dp3, bool invalid)
{
  string SymbolCol;

  if (invalid) {
    SymbolCol=\"Lofar_invalid\";
  } else {
    if (stateX >= stateY) {
      SymbolCol=getStateColor(stateX);
    } else {
      SymbolCol=getStateColor(stateY);
    }        	  
  }
  setValue(\"LBA\", \"backCol\", SymbolCol);
}
