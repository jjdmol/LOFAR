#!/bin/sh

echo "we are here"
cp ${srcdir}/../../src/Kernels/DelayAndBandPass.cu .
module load cuda50/toolkit/5.0.35
# Check for GPU
if ! lspci | grep -E "VGA|3D" | grep -E "ATI|NVIDIA"
then
  echo "No ATI/NVIDIA graphics cards detected on host `hostname`" \
       "-- aborting test." >&2
  exit 3
fi
