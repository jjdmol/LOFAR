log4cplus.rootLogger=DEBUG, FILE
log4cplus.logger.TRC=DEBUG, FILE

log4cplus.appender.FILE=log4cplus::FileAppender
log4cplus.appender.FILE.File=bbs-controller.log
log4cplus.appender.FILE.layout=log4cplus::PatternLayout
log4cplus.appender.FILE.layout.ConversionPattern=%D{%y%m%d %H%M%S,%q} %-5p %c{3} - %m%n
