<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.1//EN" "http://www.w3.org/TR/xhtml11/DTD/xhtml11.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en">
    <head>
        <title>LOFAR System Health Management Console :: Job Queue</title>
        <link rel="stylesheet" type="text/css" href="stylesheet.css" />
        <script type="text/javascript" src="ajax.js"></script>
        <script type="text/javascript" src="gui.js"></script>
    </head>

    <body onload="init_gui();">

        <%@ include file="menu.frag"%>

        <div class="content">
            <table>
                <thead>
                    <tr class="header">
                        <td>
                            <button onclick="start();"><img alt="start" src="images/start.gif" /> start</button>
                            <button onclick="stop();"><img alt="stop" src="images/stop.gif" /> stop</button>
                        </td>
                        <td>
                            <form id="filter" action="index.html" onsubmit="filter(); return false;">
                                <div>
                                    <input type="text" name="filter_txt" />
                                    <input type="submit" value="filter" />
                                </div>
                            </form>
                        </td>
                    </tr>
                    <tr class="header">
                        <td colspan="2">
                            select: <a href="javascript:check_all();">all</a> | <a href="javascript:uncheck_all();">none</a> | <img class="state" alt="pending" src="images/pending.gif" /> <a href="javascript:check_pending();">pending</a> | <img class="state" alt="running" src="images/running.gif" /> <a href="javascript:check_running();">running</a> | <img class="state" alt="stopped" src="images/stopped.gif" /> <a href="javascript:check_stopped();">stopped</a>
                        </td>
                    </tr>
                </thead>
                <tfoot />
                <tbody>
                    <tr>
                        <td colspan="2" style="padding: 0em 0em;">
                            <div id="queue_container">
                                <table id="queue">
                                    <tr>
                                        <td>
                                            <br /><br /><br /><br />
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>
                                            loading job data, please wait...
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>
                                            <br /><br /><br />
                                        </td>
                                    </tr>
                                </table>
                            </div>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
    </body>
</html>
