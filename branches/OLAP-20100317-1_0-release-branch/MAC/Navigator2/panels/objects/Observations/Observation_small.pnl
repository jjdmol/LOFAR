    myTable.currentCell(i,0);
    if (dynContains(highlight,myTable.cellValueRC(i,\"Name\"))) {
      myTable.cellBackCol(\"Lofar_highLight\");
      lineNr=i;
    } else {
      myTable.cellBackCol(\"white\");
    }      
  }
