  LOG_DEBUG(\"Observation_small.pnl:ObservationCallback|ObjectTrigger Callback on: \"+dp1+\" trigger: \"+aTrig);
  LOG_DEBUG(\"Observation_small.pnl:ObservationCallback|Found highlight : \" + highlight + \" \"+$name);
