  if (dynContains(highlight,\"OSRack\"+osNr)) {
    bHighlight=true;
  }
  LOG_DEBUG(\"CEP_OSRack.pnl:OSRackCallback|Highlight request: \"+bHighlight);
  bgprack_highLight.visible = bHighlight;
  
}

