EP_setDP()
{
dyn_errClass err;

  string query = \"SELECT '_online.._value' FROM 'LOFAR*.**.status.childState' REMOTE 'CS010:'\";
  dyn_dyn_anytype tab;
  dpQuery(query,tab);
  DebugN(\"Found: \"+tab);
  for(int z=2;z<=dynlen(tab);z++){
    dpSetWait(tab[z][1]+\":_original.._value\", 0);
    err = getLastError();
    if (dynlen(err) > 0)
      errorDialog(err);
  }
}

