"main()
{
  if(navConfigConfigSubviewPermitted())
  {
    setValue (\"ListType\", \"enabled\", TRUE);
    int customList, SelectedItem;
    dpGet($configDatapoint + \".customList\", customList);

    if (!customList)
      SelectedItem = 0;
    else if (customList)
      SelectedItem = 1;
    else
    {
      SelectedItem = 0;
      dpSet($configDatapoint + \".customList\", 1);
    }
    setValue (\"ListType\", \"number\", SelectedItem);
  }
  else
  {
    setValue (\"ListType\", \"enabled\", FALSE);
  }
}" 0

