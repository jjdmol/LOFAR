 E E "main()
{

  if(navConfigConfigSubviewPermitted())
  {
    ListType.enabled = TRUE;
    dpConnect(\"NavViewListSetConfigPermission\", $configDatapoint + \".customList:_online.._value\");
    int floatDecimals;
    string selectionQuery,selectionQueryDPE,Title;
    dpGet($configDatapoint + \".Title\", Title);
    dpGet($configDatapoint + \".floatDecimals\", floatDecimals);
    dpGet($configDatapoint + \".selectionQuery\", selectionQuery);
    dpGet($configDatapoint + \".selectionQueryDPE\", selectionQueryDPE);
    txt_title.text = Title;
    txt_dpe.text = selectionQueryDPE;
    txt_dp.text = selectionQuery;
    cbb_nrOfDecimals.selectedPos = floatDecimals + 1;
  }
  else
  {
    ListType.enabled = FALSE;
  }

}

NavViewListSetConfigPermission(string dp1, bool customList)
{
  if(navConfigConfigSubviewPermitted() && customList)
  {
    txt_dpe.enabled = TRUE;
    txt_dp.enabled = TRUE;
    txt_title.enabled = TRUE;
    cbb_nrOfDecimals.enabled = TRUE;
    btn_apply.enabled = TRUE;
  }
  else
  {
    txt_dpe.enabled = FALSE;
    txt_dp.enabled = FALSE;
    txt_title.enabled = FALSE;
    cbb_nrOfDecimals.enabled = FALSE;
    btn_apply.enabled = FALSE;
  }
  
}" 0

