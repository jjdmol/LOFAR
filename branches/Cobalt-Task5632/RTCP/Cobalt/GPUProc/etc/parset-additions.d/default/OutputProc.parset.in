# $Id$

# The user with which to log into the storage cluster. If empty, is set to $USER.
Cobalt.OutputProc.userName = 

# The public & private keys used for logging in.
# If empty, ~/.ssh/id_rsa (.pub) and ~/.ssh/id_dsa (.pub) are tried.
Cobalt.OutputProc.sshPublicKey =
Cobalt.OutputProc.sshPrivateKey =

# The executable of the process to start.
# which thus needs to be found in $PATH after login.
#TODO: install outputProc on all CEP2 nodes locally under /data/home/lofarsys/lofar_versions/Cobalt-XYZ/
Cobalt.OutputProc.executable = source /data/home/lofarsys/production/lofar_cobalt/lofarinit.sh; outputProc

# The directory where OutputProc can find the StaticMetaData
Cobalt.OutputProc.StaticMetaDataDirectory = /data/home/lofarsys/production/lofar_cobalt/etc

