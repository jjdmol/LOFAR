      updateDaemon(\"\",0,\"\",\"\",\"\",true);
    }
  } else {
    LOG_ERROR($name+\"_small.pnl:main|Couldn't find DP to connect to: \"+baseDP);
  }	
  // pass baseDP to selfstate Object to work with
  setValue(\"selfState.light\",\"toolTipText\",baseDP);
  
  // connect for selfUpdates
  showSelfState(baseDP);
}

updateDaemon(string dp1, unsigned processID,
             string dp2, string logMsg,
             string dp3, bool invalid)
{
  string col=\"Lofar_operational\";
  if (invalid) {
    if (dp1 == \"\" ) {
      col = \"Lofar_dpOffline\";
    } else {
      col = \"Lofar_invalid\";
    }
  } else if (processID == 0) {
    col = \"Lofar_off\";
  }
  myTable.deleteAllLines();
  myTable.appendLine(\"logMsg\",logMsg);  
  myTable.cellBackColRC(0,\"Active\",col);
