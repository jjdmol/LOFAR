echo "Asynchronous"> AsyncTest
for sz in 1 3 10 30 100 300 1000 3000 10000 
do
for factor in 10 13 16 20 25 31 40 50 63 79
do
echo "define" > run.in
res=$(echo "$sz * $factor"|bc)
echo "        fixedsize="$res"," >> run.in
echo "        synchronous=0" >> run.in
echo ";" >> run.in
echo "run 3003;" >> run.in
echo "quit" >> run.in
./run.sh >> AsyncTest
done
done
