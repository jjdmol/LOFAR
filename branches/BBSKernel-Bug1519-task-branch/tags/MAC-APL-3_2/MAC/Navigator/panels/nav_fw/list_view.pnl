    strreplace(temp, $datapoint, \"\");
    TableProperties.appendLine(\"propertyName\", temp , \"value\", \"\");
    finalOutput[i] = finalOutput[i] + \":_online.._value\";
  }
  dyn_string propNames;
  connectTable(finalOutput);
