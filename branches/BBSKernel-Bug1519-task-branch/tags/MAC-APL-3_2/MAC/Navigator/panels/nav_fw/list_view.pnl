                                    datapointPath + searchDP, 0, 0, TRUE, FALSE);

  dyn_string finalOutput = dynPatternMatch(\"*\"+searchDPE,output);
  for(int i=1 ; i<=dynlen(finalOutput) ; i++)
  {
