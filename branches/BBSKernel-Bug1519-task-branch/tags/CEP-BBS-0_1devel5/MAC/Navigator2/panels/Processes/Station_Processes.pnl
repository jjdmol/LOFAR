"main()
{
  
  // empty the processesList
  if (dpExists(DPNAME_NAVIGATOR + g_navigatorID + \".processesList\")) {
    dpSet(DPNAME_NAVIGATOR + g_navigatorID + \".processesList\",makeDynString(\"\"));
  }  
  
  // Initialise the Panel
  navPanel_initPanel(\"fw_viewBox\");

  baseDP = g_currentDatapoint;
  syst   = dpSubStr(baseDP,DPSUB_SYS);
    
  // set the process selectable items
  Station_Processes_initList();
  
  // connect to trigger to get a signal if some highlighting should be done.
  dpConnect( \"TriggerCallback\",true,DPNAME_NAVIGATOR + g_navigatorID + \".trigger\");
}

void TriggerCallback(string dp1, bool aTrig) {
  
  LOG_DEBUG(\"Station_Processes.pnl:Trigger Callback|entered with on: \"+dp1+\" trigger: \"+aTrig);
  LOG_DEBUG(\"Station_Processes.pnl:Trigger Callback|Found strHighlight : \" + strHighlight);
  
  // empty highlight
  dynClear(highlight);
  
  // highlights for this screen can be triggered by :
  // Observations
  // processes
  // hardware
  dyn_string obsMatch=dynPatternMatch(\"Observation*\",strHighlight);
  dyn_string processMatch=dynPatternMatch(\"LogProcessor\",strHighlight);
  dynAppend(processMatch,dynPatternMatch(\"ServiceBroker\",strHighlight));
  dynAppend(processMatch,dynPatternMatch(\"CTStartDaemon\",strHighlight));
  dynAppend(processMatch,dynPatternMatch(\"SASGateway\",strHighlight));
  dynAppend(processMatch,dynPatternMatch(\"RSPDriver\",strHighlight));
  dynAppend(processMatch,dynPatternMatch(\"TBBDriver\",strHighlight));
  dynAppend(processMatch,dynPatternMatch(\"AMCServer\",strHighlight));
  dynAppend(processMatch,dynPatternMatch(\"CalServer\",strHighlight));
  dynAppend(processMatch,dynPatternMatch(\"BeamServer\",strHighlight));
  dynAppend(processMatch,dynPatternMatch(\"HardwareMonitor\",strHighlight));
  dynAppend(processMatch,dynPatternMatch(\"SoftwareMonitor\",strHighlight));
  dynAppend(processMatch,dynPatternMatch(\"TemperatureMonitor\",strHighlight));
  dynAppend(processMatch,dynPatternMatch(\"StationCtrl\",strHighlight));
  dynAppend(processMatch,dynPatternMatch(\"DigBoardCtrl\",strHighlight));
  dynAppend(processMatch,dynPatternMatch(\"BeamCtrl\",strHighlight));
  dynAppend(processMatch,dynPatternMatch(\"CalCtrl\",strHighlight));
  dynAppend(processMatch,dynPatternMatch(\"TBBCtrl\",strHighlight));
  dynAppend(processMatch,dynPatternMatch(\"MACInfoServer\",strHighlight));
  
 
  LOG_DEBUG(\"Station_Processes.pnl:Trigger Callback|from strHighlight ObsMatch found: \"+obsMatch);
//  LOG_DEBUG(\"Station_Processes.pnl:Trigger Callback|from strHighlight stationMatch found: \"+stationMatch);
  LOG_DEBUG(\"Station_Processes.pnl:Trigger Callback|from strHighlight processMatch found: \"+processMatch);
  
  //get processes participating in Observation
//  for (int i = 1; i<= dynlen(obsMatch); i++) {
    // get the real name from the claimed datapoint
//    string ObsDP=claimManager_nameToRealName(\"LOFAR_ObsSW_\"+obsMatch[i]);
    
//    LOG_DEBUG(\"Station_Processes.pnl:Trigger Callback|Found ObsDP: \" + ObsDP);             
//    if (ObsDP != \"\") {
      // look if that name is available in the Observation List
//      int i = dynContains(g_observations[\"DP\"],ObsDP);
//      if ( i > 0) {
        // get the Stationlist from that observation
//    	string sts=g_observations[\"STATIONLIST\"][i];
//        LOG_DEBUG(\"Station_Processes.pnl:Trigger Callback|Found Stationlist for this Observation: \"+ sts);
        // add stationlist to highlight.
//        dyn_string stations = strsplit(sts,\",\");
//        if (dynlen(stations) > 0) {
//          dynAppend(highlight,stations);
//        }
//      }
//    }
//  } 
  
  //get stations from choice
//  if (dynlen(stationMatch) > 0) {
//    dynAppend(highlight,stationMatch);
