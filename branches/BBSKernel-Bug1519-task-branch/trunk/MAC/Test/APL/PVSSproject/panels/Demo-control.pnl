LANG:1 3 Set
"main()
{
dyn_errClass err;

  string frequency=TextFieldWGfrequency.text;
  string amplitude=TextFieldWGamplitude.text;
  string samplePeriod=TextFieldWGsamplePeriod.text;

  dpSetWait(\"System1:PAC_LogicalDeviceScheduler_WaveFormGenerator_frequency.:_original.._value\",frequency);
  err = getLastError();
  if (dynlen(err) > 0)
    errorDialog(err);

  dpSetWait(\"System1:PAC_LogicalDeviceScheduler_WaveFormGenerator_amplitude.:_original.._value\",amplitude);
  err = getLastError();
  if (dynlen(err) > 0)
    errorDialog(err);

  dpSetWait(\"System1:PAC_LogicalDeviceScheduler_WaveFormGenerator_samplePeriod.:_original.._value\",samplePeriod);
  err = getLastError();
  if (dynlen(err) > 0)
    errorDialog(err);

}
