      setValue(\"streamObject0\", \"foreCol\", \"Lofar_dpOffline\");
    }
    if (navFunct_dpReachable(CEPDBName) && dynlen(connectTo1)>0 ){
      if (dpConnect(\"streamCB1\", connectTo1) == -1) {
        setValue(\"streamObject1\", \"foreCol\", \"Lofar_dpdoesnotexist\");
      }
    } else {
