    if (navFunct_dpReachable(CEPDBName) && dynlen(connectTo0)>0 ){
      if (dpConnect(\"streamCB0\", connectTo0) == -1) {
        setValue(\"streamObject0\", \"foreCol\", \"Lofar_dpdoesnotexist\");
