debugopt.var:	 --with-optimize='-ggdb -O4'

prof.var: --with-optimize='-ggdb -pg'

profopt.var: --with-optimize='-O4 -pg'

debug-std.variant.conf: $(standard) --with-optimize='-ggdb -DDMI_USE_STD_ALLOC'

