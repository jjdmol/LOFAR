#!/bin/sh

# Set srcdir if needed.
# It might not be set if the .run file is executed directly instead of
# by 'make check'.
if [ "$srcdir" = "" ]; then
  srcdir=../../../test
fi

# Create the ParmDB tables for tPredSolv.
$srcdir/makeDemoParmDB tPredSolv

$LOFAR_CHECKTOOL ./tPredSolv $USER /opt/lofar/data/BBS2/demo3.MS_p1 tPredSolv_tmp.demo_instr tPredSolv_tmp.demo_sky
status=$?
exit $status

