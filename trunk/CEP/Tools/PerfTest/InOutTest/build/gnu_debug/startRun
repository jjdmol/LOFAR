echo "Work: add 1 to 1 element" > NonShared/Work1El.out
for sz in 1 3 10 30 100 300 1000 3000 10000 
do
for factor in 10 13 16 20 25 31 40 50 63 79
do
echo "define" > run.in
res=$(echo "$sz * $factor/8"|bc)
echo "        fixedsize="$res"," >> run.in
cat runscripts/runend.txt >> run.in
./run.sh >> NonShared/Work1El.out
done
done
cat run.in >> NonShared/Work1El.out
