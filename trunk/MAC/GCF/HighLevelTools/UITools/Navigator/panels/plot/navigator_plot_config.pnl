EP_textFieldIn()
{
dyn_errClass err;

  if( !dpExists( \"nav_pnl.selected_gnuplot\"))
  {
    setValue(\"\", \"color\", \"_dpdoesnotexist\");
    return;
  }

  dpConnect(\"EP_textFieldInCB\",
            \"nav_pnl.selected_gnuplot\");
  err = getLastError();
  if (dynlen(err) > 0)
    setValue(\"\", \"color\", \"_dpdoesnotexist\");

}


EP_textFieldInCB(string dp, string DP_name)
{
	string font_size, screensize;
	int number_of_plots;
	
	if( !dpExists( \"System1:\" + $DPReference + \".number_of_plots\") ||
			!dpExists( \"System1:nav_pnl.font_size\") ||
			!dpExists( \"System1:nav_pnl.screensize\"))
    return -1;
	
	dpGet(\"System1:\" + $DPReference + \".number_of_plots\", number_of_plots);
	dpGet(\"System1:nav_pnl.font_size\", font_size);
	dpGet(\"System1:nav_pnl.screensize\", screensize);
	

  setValue(\"txt_selected_gnuplot\", \"text\", DP_name);
  
  setValue(\"ComboBox_font_size\", \"text\", font_size);
  setValue(\"ComboBox_number_of_plots\", \"text\", number_of_plots);
  setValue(\"ComboBox_screensize\", \"text\", screensize);

}
