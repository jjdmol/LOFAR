//////////////////////////////////////////////////////////////////////////////////
//
// FunctionName: CheckTimeInterval
// Function    : Check the time interval for a tred
// 
///////////////////////////////////////////////////////////////////////////////////
CheckTimeInterval(int Days, int Hours, int Minutes, int Seconds, string TrendNumber)
{
  if ((0==Days) && (0==Hours) && (0==Minutes) && (Seconds<2))
  { 
    dpSet($configDatapoint + \".\"+TrendNumber+\".Minutes\", 5);
    dpSet($configDatapoint + \".\"+TrendNumber+\".Seconds\", 2);
  }
}


//////////////////////////////////////////////////////////////////////////////////
//
// FunctionName: ArrangeTrendPlots
// Function    : Arrange the trend-plots in the panel
// 
///////////////////////////////////////////////////////////////////////////////////
ArrangeTrendPlots(string dp1, bool StandardTrend, string dp2, bool Trend2Active, string dp3, bool DedicatedTrend)
{
	string Trend1Title, Trend2Title;
  //Standard Trend-plot on dpElement
  if ((dpGetElementName($datapoint)!=\"\") && StandardTrend)
  {
    string hulp = dpSubStr($datapoint, DPSUB_DP_EL_CONF_DET_ATT);
    if ((20==dpElementType(hulp)) || (21==dpElementType(hulp)) ||  (22==dpElementType(hulp)) || (23==dpElementType(hulp)))
    {
      dpSet(\"__nav_SingleElement_Trend.1.Trace1.dpName\", dpSubStr($datapoint, DPSUB_DP_EL_CONF_DET_ATT));
      dpGet(\"__nav_SingleElement_Trend.1.Title\", Trend1Title);
      setValue(\"Trend1_txt_Title\", \"text\", Trend1Title);
		  dpConnectElement2Trend();
 	    setTrend1Properties();
		  setTrendVisible(1, TRUE);
		  setTrendVisible(2, FALSE);
      dpConnect(\"CalculateTimeInterval\", \"__nav_SingleElement_Trend.1.Days\",
                                         \"__nav_SingleElement_Trend.1.Hours\",
                                         \"__nav_SingleElement_Trend.1.Minutes\",
                                         \"__nav_SingleElement_Trend.1.Seconds\");
    }
    else
    {
      DebugN(\"####### This is an illegal DPE type  #################\");
    }

  }
  //Standard Trend-plot on dpType
  else if ((dpGetElementName($datapoint)==\"\") && StandardTrend)
  {
    if (Trend2Active)  // trend-view with trend-plot 1 and 2
    {
	    dpGet($configDatapoint + \".1.Title\", Trend1Title);
 	    dpGet($configDatapoint + \".2.Title\", Trend2Title);
	    setValue(\"Trend1_txt_Title\", \"text\", Trend1Title);
 	    setValue(\"Trend2_txt_Title\", \"text\", Trend2Title);
      setValue(\"Trend1\", \"scale\", 1, 1);
			dpConnectTrend(1,1);			//Trend1, Trace1
			dpConnectTrend(1,2);
			dpConnectTrend(1,3);
			dpConnectTrend(1,4);
			dpConnectTrend(1,5);
			dpConnectTrend(1,6);
			dpConnectTrend(1,7);
			dpConnectTrend(1,8);
 			dpConnectTrend(2,1);			//Trend2, Trace1
			dpConnectTrend(2,2);
			dpConnectTrend(2,3);
			dpConnectTrend(2,4);
			dpConnectTrend(2,5);
			dpConnectTrend(2,6);
			dpConnectTrend(2,7);
			dpConnectTrend(2,8);
			dpConnect(\"DisplayTitle\", $configDatapoint + \".1.Title\");	
		  dpConnect(\"DisplayTitle\", $configDatapoint + \".2.Title\");	
			setTrendVisible(1, TRUE);
			setTrendVisible(2, TRUE);
      dpConnect(\"CalculateTimeInterval\", $configDatapoint + \".1.Days\",
                                         $configDatapoint + \".1.Hours\",
                                         $configDatapoint + \".1.Minutes\",
                                         $configDatapoint + \".1.Seconds\");

      dpConnect(\"CalculateTimeInterval\", $configDatapoint + \".2.Days\",
                                         $configDatapoint + \".2.Hours\",
                                         $configDatapoint + \".2.Minutes\",
                                         $configDatapoint + \".2.Seconds\");
    }
    else
    {	// trend-view with trend-plot 1
 	    dpGet($configDatapoint + \".1.Title\", Trend1Title);
	    setValue(\"Trend1_txt_Title\", \"text\", Trend1Title);
			dpConnectTrend(1,1);			//Trend1, Trace1
			dpConnectTrend(1,2);
			dpConnectTrend(1,3);
			dpConnectTrend(1,4);
			dpConnectTrend(1,5);
			dpConnectTrend(1,6);
			dpConnectTrend(1,7);
			dpConnectTrend(1,8);
			setTrendVisible(1, TRUE);
			dpConnect(\"DisplayTitle\", $configDatapoint + \".1.Title\");	
			setTrend1Properties();     //retrieve sizes and scale to fit
		  setTrendVisible(2, FALSE);
      dpConnect(\"CalculateTimeInterval\", $configDatapoint + \".1.Days\",
                                         $configDatapoint + \".1.Hours\",
                                         $configDatapoint + \".1.Minutes\",
                                         $configDatapoint + \".1.Seconds\");
    }
  }
  else if ((!StandardTrend) && (DedicatedTrend))     //Dedicated Trend-plot panel
  {
    string DedicatedPanelName;
    dpGet($configDatapoint + \".DedicatedPanelName\", DedicatedPanelName);
    DebugN(\"Load dedicated_trend.panel: \" + DedicatedPanelName);    
  }
} 


/////////////////////////////////////////////////////////////////////
//
// Function: Calculates the timeinterval to display in the trend display
// FunctionName: CalculateTimeInterval
//
/////////////////////////////////////////////////////////////////////
CalculateTimeInterval(string dp1, int Days, string dp2, int Hours,
                      string dp3, int Minutes, string dp4, int Seconds)
{
  int TrendNumber;
  time interval, verschil, starttime, end;

  getTrendNumber(dp1, TrendNumber);
  getValue(\"Trend\"+TrendNumber, \"visibleTimeRange\", 0, starttime, end);
  interval=makeTime(year(starttime), month(starttime), day(starttime), hour(starttime)+Hours, minute(starttime)+Minutes, second(starttime)+Seconds);
  setValue(\"Trend\" + TrendNumber, \"trendStop\");
  setValue(\"Trend\" + TrendNumber, \"timeInterval\", interval-starttime);
  setValue(\"Trend\" + TrendNumber, \"trendRun\");
}


/////////////////////////////////////////////////////////////////////
//
// Function: Display the Title of a trend, with and without given
//           TrendNumber in the dpName. 
// FunctionName: DisplayTitle
//
/////////////////////////////////////////////////////////////////////
void DisplayTitle (string dp1, string Title)
{
  int TrendNumber;
  getTrendNumber(dp1, TrendNumber);
  setValue(\"Trend\" + TrendNumber + \"_txt_Title\", \"text\", Title);
}


/////////////////////////////////////////////////////////////////////
//
// Function: connect element to trend
// FunctionName: dpConnectTrend
//
/////////////////////////////////////////////////////////////////////
void dpConnectTrend(int TrendNumber, int TraceNumber)
{
	dpConnect(\"TracePlot\", $configDatapoint + \".\" + TrendNumber + \".Trace\" + TraceNumber + \".dpName\",
												 $configDatapoint + \".\" + TrendNumber + \".Trace\" + TraceNumber + \".AutoScale\",
												 $configDatapoint + \".\" + TrendNumber + \".Trace\" + TraceNumber + \".ScaleMin\",
												 $configDatapoint + \".\" + TrendNumber + \".Trace\" + TraceNumber + \".ScaleMax\",
												 $configDatapoint + \".\" + TrendNumber + \".Trace\" + TraceNumber + \".LegendName\");
}


/////////////////////////////////////////////////////////////////////
//
// Function: connects an element to trend
// FunctionName: dpConnectElement2Trend
//
/////////////////////////////////////////////////////////////////////
void dpConnectElement2Trend()
{
	ConnectTrace(1);
  setValue(\"Trend1\", \"curveVisible\", \"curve2\", FALSE);
  setValue(\"Trend1\", \"curveScaleVisibility\", \"curve2\", FALSE);	
  setValue(\"Trend1\", \"curveVisible\", \"curve3\", FALSE);
  setValue(\"Trend1\", \"curveScaleVisibility\", \"curve3\", FALSE);	
  setValue(\"Trend1\", \"curveVisible\", \"curve4\", FALSE);
  setValue(\"Trend1\", \"curveScaleVisibility\", \"curve4\", FALSE);	
  setValue(\"Trend1\", \"curveVisible\", \"curve5\", FALSE);
  setValue(\"Trend1\", \"curveScaleVisibility\", \"curve5\", FALSE);	
  setValue(\"Trend1\", \"curveVisible\", \"curve6\", FALSE);
  setValue(\"Trend1\", \"curveScaleVisibility\", \"curve6\", FALSE);	
  setValue(\"Trend1\", \"curveVisible\", \"curve7\", FALSE);
  setValue(\"Trend1\", \"curveScaleVisibility\", \"curve7\", FALSE);	
  setValue(\"Trend1\", \"curveVisible\", \"curve8\", FALSE);
  setValue(\"Trend1\", \"curveScaleVisibility\", \"curve8\", FALSE);	
}


/////////////////////////////////////////////////////////////////////
//
// Function: set a specific Trend graphical object to a certain state
// FunctionName: setTrendVisible
//
/////////////////////////////////////////////////////////////////////
void setTrend1Properties()
{
  float Trend1Height, Trend1Width, PanelWidth, PanelHeight;
  int Trend1_x, Trend1_y;

  panelSize(\"\", PanelWidth, PanelHeight);
  Trend1Width = 941;
  Trend1Height = 351;
  getValue(\"Trend1\", \"position\", Trend1_x, Trend1_y);
  setValue(\"Trend1\", \"scale\", (PanelWidth-8 - Trend1_x)/(Trend1Width), (PanelHeight-5 - Trend1_y)/(Trend1Height));
}


/////////////////////////////////////////////////////////////////////
//
// Function: set a specific Trend graphical object to a certain state
// FunctionName: setTrendVisible
//
/////////////////////////////////////////////////////////////////////
void setTrendVisible(int TrendNumber, bool Visible)
{
  setValue(\"Trend\" + TrendNumber + \"_btn_LegendVisible\", \"visible\", Visible);
  setValue(\"Trend\" + TrendNumber + \"_txt_LegendVisible\", \"visible\", Visible);
  setValue(\"Trend\" + TrendNumber + \"_btn_ZoomVisible\", \"visible\", Visible); 
  setValue(\"Trend\" + TrendNumber + \"_txt_ZoomVisible\", \"visible\", Visible);
  setValue(\"Trend\" + TrendNumber, \"visible\", Visible);
  setValue(\"Trend\" + TrendNumber + \"_txt_LabelTitle\", \"visible\", Visible);
  setValue(\"Trend\" + TrendNumber + \"_txt_Title\", \"visible\", Visible);
}


/////////////////////////////////////////////////////////////////////
//
// Function: To plot a single trace in a Trend-plot
// FunctionName: TracePlot
//
/////////////////////////////////////////////////////////////////////
void TracePlot (string dp1, string dpName, string dp2, bool AutoScale,
								string dp3, int ScaleMin,  string dp4, int ScaleMax,
								string dp5, string LegendName)
{
  //load dpName, without element
	int TraceNumber, TrendNumber;
  getTrendNumber(dp1, TrendNumber);
  getTraceNumber(dp1, TraceNumber);

  //Check for a filled in dpName, if present start configuring trace-plot  
  if ((dpName ==\"\") || ($datapoint==\"\"))        //No datapoint configured for this Trace, or no selected item in tree
  {
    setValue(\"Trend\"+TrendNumber, \"curveVisible\", \"curve\"+TraceNumber, FALSE);
    setValue(\"Trend\"+TrendNumber, \"curveScaleVisibility\", \"curve\"+TraceNumber, FALSE);
    setValue(\"Trend\"+TrendNumber, \"curveLegendName\", \"curve\"+TraceNumber, \"No datapoint configured\");
  }
  else
	{
	  //DebugN(\"Trend dpName2Connect:\"+dpName);
	  if (dpGetElementName($datapoint)!=\"\")   //SelectedItem is a datapoint element
	  { 
	    navPMLloadPropertySet(dpSubStr($datapoint, DPSUB_SYS_DP));
      setValue(\"Trend\"+TrendNumber,\"connectDirectly\", \"curve\"+TraceNumber, $datapoint + \":_online.._value\");
	  }
	  else //SelectedItem is a datapoint (not element)
	  {
	    string dpToConnect = navViewRetrieveDpName($datapoint, dpName);
	    //if (dpAccessable($datapoint + dpName))
	    if (dpAccessable(dpToConnect))
	    {
        
        navPMLloadPropertySet(dpToConnect);
        setValue(\"Trend\"+TrendNumber, \"disconnectDirectly\", \"curve\"+TraceNumber);
        setValue(\"Trend\"+TrendNumber, \"curveRemoveData\", \"curve\"+TraceNumber, getCurrentTime());
		    setValue(\"Trend\"+TrendNumber, \"connectDirectly\", \"curve\"+TraceNumber, dpToConnect + \":_online.._value\");
        setValue(\"Trend\"+TrendNumber, \"curveLegendName\", \"curve\"+TraceNumber, LegendName);
        setValue(\"Trend\"+TrendNumber, \"curveVisible\", \"curve\"+TraceNumber, TRUE);
        setValue(\"Trend\"+TrendNumber, \"curveScaleVisibility\", \"curve\"+TraceNumber, TRUE);
        setValue(\"Trend\"+TrendNumber, \"curveAutoscale\", \"curve\"+TraceNumber, AutoScale);
      }
      else
      {
        setValue(\"Trend\"+TrendNumber, \"curveLegendName\", \"curve\"+TraceNumber, \"Resource not accessable\");
      }
    }
    if (AutoScale==FALSE)
	  {
      if (ScaleMin==ScaleMax)
        ScaleMax = ScaleMin + 10;
      //setValue(\"Trend\"+TrendNumber, \"curveMin\", \"curve\"+TraceNumber, ScaleMin);
      setValue(\"Trend\"+TrendNumber, \"curveMax\", \"curve\"+TraceNumber, ScaleMax);
	  }
  }
}


