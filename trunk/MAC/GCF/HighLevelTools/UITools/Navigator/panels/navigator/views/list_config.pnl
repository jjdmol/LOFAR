"main()
{
  int customList, SelectedItem;

	if (dpExists($configDatapoint + \".customList\"))
	{
    dpGet($configDatapoint + \".customList\", customList);

    if (!customList)
      SelectedItem = 0;
    else if (customList)
      SelectedItem = 1;
    else
    {
      SelectedItem = 0;
      dpSet($configDatapoint + \".customList\", 1);
    }
    setValue (\"RadioButton2\", \"number\", SelectedItem);
  }
  else
  {
    setValue (\"RadioButton2\", \"enabled\", FALSE);
  }

  if (dpGetElementName($datapoint)!=\"\")
  {
    setValue (\"RadioButton2\", \"enabled\", FALSE);
  }

    

