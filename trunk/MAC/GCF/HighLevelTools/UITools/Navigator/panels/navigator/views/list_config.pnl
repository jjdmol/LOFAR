"main()
{
  // Is it a datapoint element? then disable control
	// This includes that this control is only suitable for DP-Types
  dyn_string content = makeDynString(\"10\",\"20\",\"30\",\"40\",\"50\",\"75\",\"100\",\"150\",\"200\");
  
  if(!dpExists($datapoint))
  {
    ComboBox.backCol = \"DpDoesNotExist\";  
  }
	else if (dpGetElementName($datapoint)!=\"\")
  {
    setValue (\"\", \"enabled\", FALSE);
  }
  else
  {
    int selectionAmount;
    dpGet($configDatapoint + \".maxAmountToRetrieve\", selectionAmount);
    for(int i=1 ; i<=dynlen(content); i++)
    {
      ComboBox.appendItem(content[i]);
      if (selectionAmount==content[i])
 	  		{
	  			ComboBox.selectedPos = ComboBox.itemCount;
	  	  } 
    }
  }
}
