"main()
{
  if (ComboBox1.selectedPos==1)          //110-190MHz
  {
	dpSet($datapoint + \".filSelA\", FALSE);
	dpSet($datapoint + \".filSelB\", FALSE);
  }
  else if (ComboBox1.selectedPos==2)     //170-230MHz
  {
	  dpSet($datapoint + \".filSelA\", FALSE);
  	dpSet($datapoint + \".filSelB\", TRUE);
  }
  else if (ComboBox1.selectedPos==3)     //210-250MHz
  {
  	dpSet($datapoint + \".filSelA\", TRUE);
  	dpSet($datapoint + \".filSelB\", FALSE);
  }
  else if (ComboBox1.selectedPos==4)     //Filter OFF
  {
  	dpSet($datapoint + \".filSelA\", TRUE);
  	dpSet($datapoint + \".filSelB\", TRUE);
  }

