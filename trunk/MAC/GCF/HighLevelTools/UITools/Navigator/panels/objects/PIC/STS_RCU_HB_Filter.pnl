"main()
{

  if(dpAccessable($datapoint + \".filSelA\") && 
     dpAccessable($datapoint + \".filSelB\"))
	{
    dpConnect(\"ControlHBFilter\", $datapoint + \".filSelA:_online.._value\",
                                 $datapoint + \".filSelB:_online.._value\",
                                 $datapoint + \".filSelB:_online.._invalid\");
  }
  else
  {
    setValue(\"rect_filter\", \"backCol\", \"_dpdoesnotexist\");
  }
}


ControlHBFilter(string dp1, bool filSelA,
                string dp2, bool filSelB,
                string dp3, bool invalid)
{
//  if(!invalid)
 // {
    ComboBox1.enabled = TRUE;
    if (!filSelA && !filSelB)
    {
      ComboBox1.selectedPos=1;
      setValue(\"rect_filter\", \"backCol\", \"Lofar_device_active\");
      DebugN(\"NOT filSelA &&  NOT filSelB\");
    }
    else if (!filSelA && filSelB)
    {
      ComboBox1.selectedPos=2;
      setValue(\"rect_filter\", \"backCol\", \"Lofar_device_active\");
      DebugN(\"NOT filSelA && filSelB\");
    }
    else if (filSelA && !filSelB)
    {
      ComboBox1.selectedPos=3;
      setValue(\"rect_filter\", \"backCol\", \"Lofar_device_active\");
      DebugN(\"filSelA &&  NOT filSelB\");
    }
    else if (filSelA && filSelB)
    {
      ComboBox1.selectedPos=4;
      setValue(\"rect_filter\", \"backCol\", \"grey\");
      DebugN(\"filSelA && filSelB\");
    }
/*  }
  else
  {
    ComboBox1.enabled = FALSE;
    setValue(\"rect_filter\", \"backCol\", \"_dpdoesnotexist\");
