[RSPDRIVER]

N_RSPBOARDS=1
N_BLPS=1
SYNC_INTERVAL=1.0
SW_CLOCK=0
SOFTPPSF=0
MAC_BASE=1 # Start from address: 00:00:00:00:00:MAC_BASE

IF_NAME=eth0

#
# Make sure that initially the
# beamlet statistics match the subband statistics
#
IDENTITY_WEIGHTS=1

#
# normal update sequence
#
WRITE_BF=1
WRITE_SS=1
WRITE_RCU=1
READ_STATUS=1
READ_ST=1
WRITE_WG=1
READ_VERSION=1

#
# extra updates for testing purposes
#
READ_BF=0
READ_SS=0
READ_RCU=0
READ_WG=0

