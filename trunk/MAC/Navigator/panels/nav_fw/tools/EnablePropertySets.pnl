"main()
{
    string datapoint;
    dyn_dyn_anytype tab;
    dyn_string output, temp;
    dyn_string dpList;
    getValue(\"txt\", \"text\", datapoint);
    dpQuery(\"SELECT '_online.._value' FROM '\" + datapoint + \"'\", tab);
    string temp;
    int k=1;
    for(int i=2 ; i<=dynlen(tab) ; i++)
    {
      temp = dpSubStr(tab[i][1], DPSUB_DP);
      if(dynContains(output,temp)==0)
      {
        output[k]= temp;
        k++;
      }
    }

   ////////////////////////////////////////
   //
   string datapoint;
   string dpType;
   setValue(\"txt_total\", \"text\", dynlen(output));
   for(int i=1; i<=dynlen(output); i++)
   {
     datapoint = output[i];
     dpType = dpTypeName(datapoint);
     if(dpType[0] == \"T\")
     {
       if(!dpExists(datapoint + \"__enabled\"))
       {
         dpCreate(datapoint + \"__enabled\", \"GCFPaPsEnabled\");
       }
       dpSetWait(datapoint + \"__enabled.:_original.._value\", \"perm-autoloaded|\" + dpType);
       DebugN(\"## \" + datapoint + \"    |content: \" +  \"perm-autoloaded|\"+ dpType);
       
     }
   setValue(\"txt_current\", \"text\", i);
   }
   DebugN(\"## Done\");
}" 0
