"$datapoint"
"main()
{
 
  baseDP=$datapoint;
 
 // prepare logMsg logging
  selectedDP=baseDP + \".logMsg\";

  if (dpExists(selectedDP)) {
	 dpConnect(\"updateLogging\",selectedDP);
	 showLogging();
	} else {
	  LOG_ERROR(\"TBBController couldn't connect to logMsg logging\");
	}
	
	//prepare to keep track of Status
	
	if (dpExists(baseDP+\".currentAction\")) {
 	  dpConnect(\"updateStatus\",baseDP+\".currentAction\",
	                          baseDP+\".connected\",
	                          baseDP+\".connected:_online.._invalid\");
	} else {
	  setValue(\"txt_currentAction\", \"backCol\", \"_dpdoesnotexist\");
	  setValue(\"txt_currentAction\",\"text\",\"?????\");
	  setValue(\"txt_connected\",\"backCol\",\"red\");
	  LOG_ERROR(\"TBBController couldn't connect to currentAction\");
	}
	
 
 	//prepare to get connected to trigger updates
 	string triggerDP=baseDP+\".trigger\";
	if (dpExists(triggerDP+\".rcuNr\")) {
 	  dpConnect(\"updateTrigger\",triggerDP+\".table\",
	                            triggerDP+\".table:_online.._invalid\");
	  showTrigger();
	} else {
	  LOG_ERROR(\"TBBController couldn't connect to: \"+triggerDP+\".table\");
	}
 
 
 	connectTBBoards();
 
  // connect for selfUpdates
  showSelfState(baseDP);
}

void updateStatus(string dp1, string currentAction,
                  string dp2, bool connected,
                  string dp3, bool invalid) {
                   
	if (invalid) 
	{
    setValue(\"txt_currentAction\"   , \"text\", \"?????\");
    setValue(\"txt_connected\"   , \"backCol\", \"red\" );
  } else {
    setValue(\"txt_currentAction\"   , \"text\", currentAction);
    if (connected) {
      setValue(\"txt_connected\"   , \"backCol\", \"green\" );
    } else {
      setValue(\"txt_connected\"   , \"backCol\", \"red\" );
    }
  }	    
}

void connectTBBoards(){

	string syst = dpSubStr(baseDP,DPSUB_SYS);
  
  string aDP=syst+\"LOFAR_PIC_\";
  int l=0;
  for (int i = 0; i<3; i++) {
    for (int j= 0; j< 2; j++) {
      for (int k=0; k<2; k++) {
     	  string dp =  aDP+\"Cabinet\"+i+\"_Subrack\"+(2*i+j)+\"_TBBoard\"+l;
     	  if (dpExists(dp)) {
    	    dpConnect(\"updateBoardState\",dp+\".state\",
    	                                 dp+\".state:_online.._invalid\");
        } else {
	        setValue(\"txt_board\"+l, \"backCol\", \"_dpdoesnotexist\");
        }
        l++;
      }
    }
  }	
}

void updateBoardState( string dp1, int state,
                       string dp2, bool invalid) {
	
	string col;
	
	if (invalid) {
	  col = \"Lofar_invalid\";
	} else {
	  col = getStateColor(state);
	}
	// get boardnum
	
	string aDP= substr(dp1,DPSUB_DP);
		dyn_string as = strsplit(strsplit(dp1,\".\")[1],\"_\");
  string tbb = as[dynlen(as)]; 
  string aNr = substr(tbb, 7 , strlen(tbb));	
	
	setValue(\"txt_board\"+aNr,\"backCol\",col);	
}
