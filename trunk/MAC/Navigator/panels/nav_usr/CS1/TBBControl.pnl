E"string baseDP;
string selectedDP;
string selDP;

showLogging()
{
	//DebugTN(\"startLogging\");
	dyn_string dp = strsplit(dpSubStr(selectedDP,DPSUB_DP),\"_\");
	
	//DebugTN(\"startLogging 2\");

 	// get the systemname and systemnameless part of the datapoint
	string systemName = dpSubStr(selectedDP,DPSUB_SYS);
  string bareDP     = dpSubStr(selectedDP,DPSUB_DP_EL_CONF_DET_ATT);
 
 	
	//DebugTN(\"startLogging 3\");

  // clear the table from the old logging
  table_logging.deleteAllLines();


  // if the datapoint exists, try to get some of the historical data on this point
  // fill the table with it and do a dpConnect so that new changes will be added to the table 
  if(dpExists(selectedDP))
  {
		// initialize the trigger table with historical data:
		dyn_dyn_anytype tab;
		int z;
		time tStart;
		time tStop;
		tStop = getCurrentTime();
		tStart = tStop - 48*3600; // 2 days of history
	
		string query = \"SELECT ALL '_original.._value' FROM '\" + bareDP + \"' REMOTE'\"+systemName +\"' TIMERANGE(\\\"\" +
			formatTime(\"%Y.%m.%d %H:%M:%S\",tStart) + \"\\\",\\\"\" +
			formatTime(\"%Y.%m.%d %H:%M:%S\",tStop) + \"\\\",1,0) LAST 100\";
			
		//DebugTN(\"Query: \" + query);
		dpQuery(query, tab);
		LOG_DEBUG(\"Found: \" + tab + \" length: \" + dynlen(tab));
	 	 
	 	
		for(z=2;z<dynlen(tab);z++)
		{
			addLogMessage(tab[z][2]);
		}
	}
	else {
	  //DebugTN(\"NOT FOUND: wanted to get historical data from : \" + selectedDP);
	}
}

showTrigger()
{

	selDP=baseDP + \".trigger.table\";
	dyn_string dp = strsplit(dpSubStr(selDP,DPSUB_DP),\"_\");
	
 	// get the systemname and systemnameless part of the datapoint
	string systemName = dpSubStr(selDP,DPSUB_SYS);
  string bareDP     = dpSubStr(selDP,DPSUB_DP_EL_CONF_DET_ATT);
 
  // clear the table from the old logging
  table_trigger.deleteAllLines();


	//DebugTN(\"startLogging 4 selectedDP: \" + selectedDP);
  // if the datapoint exists, try to get some of the historical data on this point
  // fill the table with it and do a dpConnect so that new changes will be added to the table 
  if(dpExists(selDP))
  {
    
		// initialize the logging table with historical data:
		dyn_dyn_anytype tab;
		int z;
		time tStart;
		time tStop;
		tStop = getCurrentTime();
		tStart = tStop - 48*3600; // 2 days of history
	
		string query = \"SELECT ALL '_original.._value' FROM '\" + bareDP + \"' REMOTE'\"+systemName +\"' TIMERANGE(\\\"\" +
			formatTime(\"%Y.%m.%d %H:%M:%S\",tStart) + \"\\\",\\\"\" +
			formatTime(\"%Y.%m.%d %H:%M:%S\",tStop) + \"\\\",1,0) LAST 100\";
			
		//DebugTN(\"Query: \" + query);
		dpQuery(query, tab);
		LOG_DEBUG(\"Found: \" + tab + \" length: \" + dynlen(tab));
	 	 
	 	
		for(z=2;z<dynlen(tab);z++)
		{
			addTriggerMessage(tab[z][2]);
		}
	}
	else {
	  //DebugTN(\"NOT FOUND: wanted show historical data from : \" + selDP);
	}
}

//
// Update logging callback
//
void updateLogging(string dpe, string dpv)
{
  //DebugTN(\"updateLogging: LogMsg: \"+dpv + \" point: \" + dpe);
  if (dpv != \"\") {
    addLogMessage(dpv);
  }
}

// Update trigger callback
void updateTrigger (string dp1,string table,
                    string dp2, bool invalid) {
  
	if (!invalid && table != \"\") {
    addTriggerMessage(table);
	} 
}

//
// Add logMsg to table, We assume the tablename is table_logging
//
addLogMessage(string msg)
{
  //split lines like:
  // 13-11-06 10:06:00.519|INFO|MAC.GCF.PAL.SAL|Set value of property 'MCU001:LOFAR_PermSW_MACScheduler.OTDB.lastPoll'|GSA_Service.cc:661
  // into relevant pieces

  if (msg != \"\") {
    //DebugTN(\"addLogMessage: \" + msg);

    dyn_string msgParts;
    string dateTime=\"\";
    string level=\"\";
    string source=\"\";
    string logMsg=\"\";
    string codeLine=\"\";
    bool error=false;

    msgParts = strsplit(msg,\"|\");

    if (dynlen(msgParts) >=1) {
      dateTime = msgParts[1];
    } else {
    	error=true;
   	}
    if (dynlen(msgParts) >=2) {
      level    = msgParts[2];
    } else {
    	error=true;
   	}
    if (dynlen(msgParts) >=3) {
      source   = msgParts[3];
    } else {
    	error=true;
   	}

    if (dynlen(msgParts) >=4) {
      logMsg = msgParts[4];
    } else {
    	error=true;
   	}

    if (dynlen(msgParts) >=5) {
      codeLine = msgParts[5];
    } else {
    	error=true;
   	}
	
	
    //DebugTN(\"dateTime: \" + dateTime);
    //DebugTN(\"level: \" + level);
    //DebugTN(\"source: \" + source);
    //DebugTN(\"logMsg: \" + logMsg);
    //DebugTN(\"codeLine: \" + codeLine);

		if (!error && logMsg != \"\") {
	    table_logging.appendLine(\"time\",dateTime,\"level\",level,\"source\",source,\"message\",logMsg,\"code\",codeLine);
  	  table_logging.lineVisible(-1);
  	 }
  }
}

//
// Add triggerData to table, We assume the tablename is table_trigger
//
addTriggerMessage(string trigger)
{
  //split lines into relevant pieces

  if (trigger != \"\") {
    //DebugTN(\"addLogMessage: \" + msg);

    dyn_string msgParts;
    unsigned rcuNr;
    unsigned sequenceNr;
    string timeStamp;
    unsigned sampleNr;
    unsigned sum;
    unsigned nrSamples;
    unsigned peakValue;
    unsigned flags;

    bool error=false;

    msgParts = strsplit(trigger,\"|\");

    if (dynlen(msgParts) >=1) {
      rcuNr = msgParts[1];
    } else {
    	error=true;
   	}
    if (dynlen(msgParts) >=2) {
      sequenceNr    = msgParts[2];
    } else {
    	error=true;
   	}
    if (dynlen(msgParts) >=3) {
      timeStamp   = msgParts[3];
    } else {
    	error=true;
   	}

    if (dynlen(msgParts) >=4) {
      sampleNr = msgParts[4];
    } else {
    	error=true;
   	}

    if (dynlen(msgParts) >=5) {
      sum = msgParts[5];
    } else {
    	error=true;
   	}

    if (dynlen(msgParts) >=6) {
      nrSamples = msgParts[6];
    } else {
    	error=true;
   	}

    if (dynlen(msgParts) >=7) {
      peakValue = msgParts[7];
    } else {
    	error=true;
   	}
	
    if (dynlen(msgParts) >=8) {
      flags = msgParts[8];
    } else {
    	error=true;
   	}
	
		if (!error && trigger != \"\") {
		table_trigger.appendLine(\"rcuNr\",rcuNr,\"sequenceNr\",sequenceNr,\"time\",timeStamp,\"sampleNr\",sampleNr,\"sum\",sum,
		                         \"nrSamples\",nrSamples,\"peakValue\",peakValue,\"flags\",flags);
 	  table_trigger.lineVisible(-1);
  	 }
  }
}
