void checkPortType()
{
	dyn_string foundSwitches = dpNames(\"*\", SWITCH_TYPE_PREFIX + slPortTypes.selectedText);
	if (dynlen(foundSwitches) > 0)
	{
		btnDelete.enabled = false;
		tfStatus.text = \"Type \\\"\" + slPortTypes.selectedText + \"\\\" is in use! Cannot deleted!\";
	}
	else
	{
		tfStatus.text = \"\";
		btnDelete.enabled = true;
	}
}" 0
