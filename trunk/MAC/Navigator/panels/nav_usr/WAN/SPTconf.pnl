"main()
{
	int nrOfPorts = (int) tfNewPortType.text;
	if (nrOfPorts <= MAX_NR_OF_SWITCH_PORTS)
	{
		dyn_dyn_string xxdepes;		
		dyn_dyn_int xxdepei;
		
		// Create the data type
		
		xxdepes[1] = makeDynString (SWITCH_TYPE_PREFIX + tfNewPortType.text, \"\");
		xxdepei[1] = makeDynInt (DPEL_STRUCT);
		xxdepes[2] = makeDynString (\"\", \"location\", \"\");
		xxdepei[2] = makeDynInt (0, DPEL_STRING);
		xxdepes[3] = makeDynString (\"\", \"uptime\", \"\");
		xxdepei[3] = makeDynInt (0, DPEL_STRING);
		xxdepes[4] = makeDynString (\"\", \"name\", \"\");
		xxdepei[4] = makeDynInt (0, DPEL_STRING);
		xxdepes[5] = makeDynString (\"\", \"contact\", \"\");
		xxdepei[5] = makeDynInt (0, DPEL_STRING);
		xxdepes[6] = makeDynString (\"\", \"description\", \"\");
		xxdepei[6] = makeDynInt (0, DPEL_STRING);
		xxdepes[7] = makeDynString (\"\", \"status\", \"\");
		xxdepei[7] = makeDynInt (0, DPEL_INT);
		for (int i = 0; i < nrOfPorts; i++)
		{
			xxdepes[8  + i] = makeDynString (\"\", \"port\" + (i + 1), \"TWanSwitchPort\");
			xxdepei[8  + i] = makeDynInt (0, DPEL_TYPEREF);
		}	
	 
		// Create the datapoint type
		int n = dpTypeCreate(xxdepes, xxdepei);
		if (n == 0) 
		{
			tfStatus.text = \"Type \\\"\" + tfNewPortType.text + \"\\\" add!\";
			dyn_int types = slPortTypes.items;
			dynAppend(types, nrOfPorts);
			dynSortAsc(types);
			slPortTypes.items = types;
		}
	}
}" 0
