"main()
{
	dpTypeDelete(SWITCH_TYPE_PREFIX + slPortTypes.selectedText);	
	int curPos = slPortTypes.selectedPos;
	tfStatus.text = \"Type \\\"\" + slPortTypes.selectedText + \"\\\" deleted!\";
	slPortTypes.deletePos = slPortTypes.selectedPos;
	slPortTypes.selectedPos = (curPos > slPortTypes.itemCount	? slPortTypes.itemCount : curPos);
	checkPortType();
