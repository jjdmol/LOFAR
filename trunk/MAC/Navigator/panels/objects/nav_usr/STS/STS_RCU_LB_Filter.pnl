"//
//RCU_LB_Filter (JjS)


main()
{

  if(dpAccessable($datapoint + \".filSel0\") && 
     dpAccessable($datapoint + \".filSel1\"))
	{
    dpConnect(\"ControlLBFilter\", $datapoint + \".filSel0:_online.._value\",
                                 $datapoint + \".filSel1:_online.._value\",
                                 $datapoint + \".filSel1:_online.._invalid\",
	  														 $datapoint + \".bandSelLBA_HBA:_online.._value\",
	                               $datapoint + \".bandSelLBA_HBA:_online.._invalid\");
  }
  else
  {
    setValue(\"rect_filterLB\", \"backCol\", \"_dpdoesnotexist\");
  }
}


ControlLBFilter(string dp1, bool filSel0,
                string dp2, bool filSel1,
                string dp3, bool invalid,
                string dp4, bool notLBL,
                string dp5, bool bandSel_invalid                )
{

  if(!invalid && !bandSel_invalid)
  {
  	if(!notLBL)
		{
			ComboBoxLB.enabled = TRUE;
    	rect_filterLB.enabled = TRUE;
    	if (!filSel1 && !filSel0)
    	{
      	ComboBoxLB.selectedPos=1;    //10-80MHz
      	setValue(\"rect_filterLB\", \"backCol\", \"Lofar_device_active\");
    	}
    	else if (!filSel1 && filSel0)
    	{
      	ComboBoxLB.selectedPos=2;    //30-80MHz
      	setValue(\"rect_filterLB\", \"backCol\", \"Lofar_device_active\");
    	}
    	else if (filSel1 && !filSel0)
    	{
      	ComboBoxLB.selectedPos=3;    //Filter OFF
      	setValue(\"rect_filterLB\", \"backCol\", \"grey\");
    	}
    	else if (filSel1 && filSel0)
    	{
      	ComboBoxLB.selectedPos=3;    //Filter OFF
      	setValue(\"rect_filterLB\", \"backCol\", \"grey\");
    	}
    }
    else
    {
    	setValue(\"rect_filterLB\", \"backCol\", \"grey\");
    }
  }
  else
  {
		ComboBoxLB.enabled = FALSE;
    rect_filterLB.enabled = FALSE;
    setValue(\"rect_filterLB\", \"backCol\", \"_dpdoesnotexist\");
  }
