EP_setBackColorCB(string dpSource1,  unsigned AP0_RCU0_stat, string dpSource2,  unsigned AP0_RCU0_main,
									string dpSource3,  unsigned AP0_RCU1_stat, string dpSource4,  unsigned AP0_RCU1_main,
									string dpSource5,  unsigned AP1_RCU0_stat, string dpSource6,  unsigned AP1_RCU0_main,
									string dpSource7,  unsigned AP1_RCU1_stat, string dpSource8,  unsigned AP1_RCU1_main,
									string dpSource9,  unsigned AP2_RCU0_stat, string dpSource10, unsigned AP2_RCU0_main,
									string dpSource11, unsigned AP2_RCU1_stat, string dpSource12, unsigned AP2_RCU1_main,
									string dpSource13, unsigned AP3_RCU0_stat, string dpSource14, unsigned AP3_RCU0_main,
									string dpSource15, unsigned AP3_RCU1_stat, string dpSource16, unsigned AP3_RCU1_main,
									string dpSource17, unsigned AP0_stat,      string dpSource18, unsigned AP1_stat,
									string dpSource19, unsigned AP2_stat,      string dpSource20, unsigned AP3_stat,
									string dpSource21, unsigned BP_stat,       string dpSource22, unsigned BP_main,
									string dpSource23, unsigned Subrack_main,
									string dpSource24, bool BP_invalid)
{
	//Declaration of temporary variables
	int AP0_RCU0_health, AP0_RCU1_health, AP1_RCU0_health, AP1_RCU1_health;
	int AP2_RCU0_health, AP2_RCU1_health, AP3_RCU0_health, AP3_RCU1_health;
	int AP0_health, AP1_health, AP2_health, AP3_health;
	int BP_health;
	int Subrack_health;
	int SR_health;	// Subrack_maintenance status
  if(BP_invalid) // The information stored in the dp's are INVALID!!
  {
    setValue(\"LED_1\", \"backCol\", \"_dpdoesnotexist\");
    setValue(\"LED_1\", \"foreCol\", \"_dpdoesnotexist\");
    setValue(\"LED_2\", \"backCol\", \"_dpdoesnotexist\");
    setValue(\"LED_2\", \"foreCol\", \"_dpdoesnotexist\");
    setValue(\"LED_3\", \"backCol\", \"_dpdoesnotexist\");
    setValue(\"LED_3\", \"foreCol\", \"_dpdoesnotexist\");
    setValue(\"LED_4\", \"backCol\", \"_dpdoesnotexist\");
    setValue(\"LED_4\", \"foreCol\", \"_dpdoesnotexist\");
    setValue(\"LED_5\", \"backCol\", \"_dpdoesnotexist\");
    setValue(\"LED_5\", \"foreCol\", \"_dpdoesnotexist\");
    setValue(\"LED_6\", \"backCol\", \"_dpdoesnotexist\");
    setValue(\"LED_6\", \"foreCol\", \"_dpdoesnotexist\");
    setValue(\"LED_7\", \"backCol\", \"_dpdoesnotexist\");
    setValue(\"LED_7\", \"foreCol\", \"_dpdoesnotexist\");
    setValue(\"LED_8\", \"backCol\", \"_dpdoesnotexist\");
    setValue(\"LED_8\", \"foreCol\", \"_dpdoesnotexist\");
    setValue(\"rect_Subrack\", \"backCol\", \"_dpdoesnotexist\");
  }
  else
  {
	//Calculate the Error/Health value for each variable
	if ((AP0_RCU0_stat + AP0_RCU0_main)<1) AP0_RCU0_health=1; else AP0_RCU0_health=0;
	if ((AP0_RCU1_stat + AP0_RCU1_main)<1) AP0_RCU1_health=1; else AP0_RCU1_health=0;
	if ((AP1_RCU0_stat + AP1_RCU0_main)<1) AP1_RCU0_health=1; else AP1_RCU0_health=0;
	if ((AP1_RCU1_stat + AP1_RCU1_main)<1) AP1_RCU1_health=1; else AP1_RCU1_health=0;
	if ((AP2_RCU0_stat + AP2_RCU0_main)<1) AP2_RCU0_health=1; else AP2_RCU0_health=0;
	if ((AP2_RCU1_stat + AP2_RCU1_main)<1) AP2_RCU1_health=1; else AP2_RCU1_health=0;
	if ((AP3_RCU0_stat + AP3_RCU0_main)<1) AP3_RCU0_health=1; else AP3_RCU0_health=0;
	if ((AP3_RCU1_stat + AP3_RCU1_main)<1) AP3_RCU1_health=1; else AP3_RCU1_health=0;
	
	if (AP0_stat ==0 ) AP0_health=1; else AP0_health=0;
	if (AP1_stat ==0 ) AP1_health=1; else AP1_health=0;
	if (AP2_stat ==0 ) AP2_health=1; else AP2_health=0;
	if (AP3_stat ==0 ) AP3_health=1; else AP3_health=0;
		
	if ((BP_stat + BP_main)==0) BP_health=1; else BP_health=0;
	if (Subrack_main==0) SR_health=1; else SR_health=0;

	// Calculate the subtrack health value
	Subrack_health =	(SR_health * BP_health *	(	AP0_health * (AP0_RCU0_health + AP0_RCU1_health) +
																	AP1_health * (AP1_RCU0_health + AP1_RCU1_health) +
																	AP2_health * (AP2_RCU0_health + AP2_RCU1_health) +
																	AP3_health * (AP3_RCU0_health + AP3_RCU1_health)
																) * 100) / 8; 

	// Display the subtrack health value into the six LED's
    if (Subrack_health == 100)
  	{
      setValue(\"LED_1\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_1\", \"foreCol\", \"Lofar_device_active\");
      setValue(\"LED_2\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_2\", \"foreCol\", \"Lofar_device_active\");
      setValue(\"LED_3\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_3\", \"foreCol\", \"Lofar_device_active\");
      setValue(\"LED_4\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_4\", \"foreCol\", \"Lofar_device_active\");
      setValue(\"LED_5\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_5\", \"foreCol\", \"Lofar_device_active\");
      setValue(\"LED_6\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_6\", \"foreCol\", \"Lofar_device_active\");
      setValue(\"LED_7\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_7\", \"foreCol\", \"Lofar_device_active\");
      setValue(\"LED_8\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_8\", \"foreCol\", \"Lofar_device_active\");
    }
    else if ((Subrack_health) > 86 && ((Subrack_health) <= 100))
  	{
      setValue(\"LED_1\", \"backCol\", \"Yellow\");
      setValue(\"LED_1\", \"foreCol\", \"Yellow\");
      setValue(\"LED_2\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_2\", \"foreCol\", \"Lofar_device_active\");
      setValue(\"LED_3\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_3\", \"foreCol\", \"Lofar_device_active\");
      setValue(\"LED_4\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_4\", \"foreCol\", \"Lofar_device_active\");
      setValue(\"LED_5\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_5\", \"foreCol\", \"Lofar_device_active\");
      setValue(\"LED_6\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_6\", \"foreCol\", \"Lofar_device_active\");
      setValue(\"LED_7\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_7\", \"foreCol\", \"Lofar_device_active\");
      setValue(\"LED_8\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_8\", \"foreCol\", \"Lofar_device_active\");
    }
    else if ((Subrack_health) > 74 && ((Subrack_health) <= 86))
  	{
      setValue(\"LED_1\", \"backCol\", \"Yellow\");
      setValue(\"LED_1\", \"foreCol\", \"Yellow\");
      setValue(\"LED_2\", \"backCol\", \"Yellow\");
      setValue(\"LED_2\", \"foreCol\", \"Yellow\");
      setValue(\"LED_3\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_3\", \"foreCol\", \"Lofar_device_active\");
      setValue(\"LED_4\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_4\", \"foreCol\", \"Lofar_device_active\");
      setValue(\"LED_5\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_5\", \"foreCol\", \"Lofar_device_active\");
      setValue(\"LED_6\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_6\", \"foreCol\", \"Lofar_device_active\");
      setValue(\"LED_7\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_7\", \"foreCol\", \"Lofar_device_active\");
      setValue(\"LED_8\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_8\", \"foreCol\", \"Lofar_device_active\");
    }
    else if ((Subrack_health) > 61 && ((Subrack_health) <= 74))
  	{
      setValue(\"LED_1\", \"backCol\", \"Yellow\");
      setValue(\"LED_1\", \"foreCol\", \"Yellow\");
      setValue(\"LED_2\", \"backCol\", \"Yellow\");
      setValue(\"LED_2\", \"foreCol\", \"Yellow\");
      setValue(\"LED_3\", \"backCol\", \"Yellow\");
      setValue(\"LED_3\", \"foreCol\", \"Yellow\");
      setValue(\"LED_4\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_4\", \"foreCol\", \"Lofar_device_active\");
      setValue(\"LED_5\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_5\", \"foreCol\", \"Lofar_device_active\");
      setValue(\"LED_6\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_6\", \"foreCol\", \"Lofar_device_active\");
      setValue(\"LED_7\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_7\", \"foreCol\", \"Lofar_device_active\");
      setValue(\"LED_8\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_8\", \"foreCol\", \"Lofar_device_active\");
    }
    else if ((Subrack_health) > 49 && ((Subrack_health) <= 61))
  	{
      setValue(\"LED_1\", \"backCol\", \"Yellow\");
      setValue(\"LED_1\", \"foreCol\", \"Yellow\");
      setValue(\"LED_2\", \"backCol\", \"Yellow\");
      setValue(\"LED_2\", \"foreCol\", \"Yellow\");
      setValue(\"LED_3\", \"backCol\", \"Yellow\");
      setValue(\"LED_3\", \"foreCol\", \"Yellow\");
      setValue(\"LED_4\", \"backCol\", \"Yellow\");
      setValue(\"LED_4\", \"foreCol\", \"Yellow\");
      setValue(\"LED_5\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_5\", \"foreCol\", \"Lofar_device_active\");
      setValue(\"LED_6\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_6\", \"foreCol\", \"Lofar_device_active\");
      setValue(\"LED_7\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_7\", \"foreCol\", \"Lofar_device_active\");
      setValue(\"LED_8\", \"backCol\", \"Lofar_device_active\");
      setValue(\"LED_8\", \"foreCol\", \"Lofar_device_active\");
    }
    else if ((Subrack_health) > 36 && ((Subrack_health) <= 49))
	  {
      setValue(\"LED_1\", \"backCol\", \"Red\");
      setValue(\"LED_1\", \"foreCol\", \"Red\");
