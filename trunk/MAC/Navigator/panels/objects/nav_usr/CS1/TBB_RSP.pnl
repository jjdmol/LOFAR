"main()
{

  // strip TBBoard and add RSPBoard
  // TBBoard0 has RSPBoard 0 to the left
  // TBBoard1 has RSPBoard 2 to the left
  // TBBoard2 has RSPBoard 4 to the left 
  // and so on
  
  baseDP=$datapoint;
  referenceBaseDP=$referenceDatapoint;
  // Isolate the TBBoard part of the DP
  LOG_DEBUG(\"Datapoint: \"+baseDP);
  string sys= dpSubStr(baseDP,DPSUB_SYS);
  LOG_DEBUG(\"System: \"+sys);
  dyn_string dp  = strsplit(dpSubStr(baseDP,DPSUB_DP),\"_\");
  LOG_DEBUG(\"Splitted string:\" + dp);
  string tbb = dp[dynlen(dp)]; 
  LOG_DEBUG(\"tbb:\" + tbb);
  
  string aNr = substr(tbb, 7 , strlen(tbb));
  LOG_DEBUG(\"BoardNr:\" + aNr);
  
	// Define the anInteger of the isolated number  
 
 	tbNr=aNr;	 
  LOG_DEBUG(\"BoardNr:\" + tbNr);

  int rspNr = tbNr*2;
  if ($position == \"right\") {
		rspNr+=1;
	}
	
  setValue(\"txt_rspnr\",\"text\",\"RSP-\"+rspNr);
 
  string aS=\"RSPBoard\"+rspNr;
  LOG_DEBUG(\"aS:\" + aS);
  
  
  strreplace(baseDP,tbb,aS);
  strreplace(referenceBaseDP,tbb,aS);
  
  LOG_DEBUG(\"Composed datapoint \" + $datapoint + \": \" + baseDP);
  
  // connect for childUpdates
  showChildState(baseDP);
  // connect for selfUpdates
  showSelfState(baseDP);
}" 0
