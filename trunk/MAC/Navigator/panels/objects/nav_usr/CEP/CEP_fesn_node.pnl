void computeFESNSumAlert(string ident, dyn_dyn_anytype tab)
{
	unsigned error = 0;
 	for (int z = 2; z <= dynlen(tab); z++)
	{
	
	
	unsigned checkType=99;
	if ($FESNType == \"Input\" )
	{
	  checkType=0;
 	}
 	else if ($FESNType == \"Output\")
 	{
 	  checkType=1;
 	}
 	else if ($FESNType == \"Aux\")
 	{
 	  checkType=2;
 	} 
 	else if ($FESNType == \"Offline\")
 	{
 	  checkType=3;
 	}
 	else if ($FESNType == \"BG/L\")
 	{
 	  checkType=4;
 	}
	unsigned type;
	dpGet(strreplace(tab[z][1],\".status\",\".type\"),type);
		if (type == checkType)
	  {
			if (tab[z][2] < 0)
			{
				error++;
				break;
			}
		}
	}
	if (error > 0)
	{
    FESNborder.backCol = \"red\";
	}
	else
	{
		FESNborder.backCol = \"Lofar_device_active\";
	}
