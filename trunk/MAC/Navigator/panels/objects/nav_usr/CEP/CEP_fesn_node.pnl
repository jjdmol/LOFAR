{

  // Check for FESN types only
	string query = \"SELECT '_online.._value' FROM 'CEP_PIC_Room*_Row*_Rack*_Item*.status' REMOTE ALL WHERE _DPT=\\\"TCepPicFESN\\\" \";
	dpQueryConnectAll(\"computeFESNSumAlert\", true, \"FESNSumAlert\", query);
