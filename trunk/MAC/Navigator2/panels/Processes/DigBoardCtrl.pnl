"main()
{
  // Initialise the Panel
  navPanel_initPanel(\"fw_viewBox\");
  
  baseDP         = g_currentDatapoint;
  database = dpSubStr(baseDP,DPSUB_SYS);
  
  // set the selectable processes
  prepareProcesses();

  // trigger that the panel values are calculated and ready
  navPanel_setEvent(\"DigBoardCtrl.pnl\",\"Update\");
  
  // check if the requiered datapoint for this view are enabled and accessible
  if (dpExists(baseDP + \".connected\")) {
    dpConnect(\"updateDigBoardCtrl\", baseDP + \".connected\",
		                    baseDP + \".clock\",
		                    baseDP + \".connected:_online.._invalid\");
  } else {
    LOG_ERROR(\"DigBoardCtrl.pnl:main|Couldn't find DP to connect to: \"+baseDP);
    setValue(\"enabledText\", \"backCol\", \"_dpdoesnotexist\");
    setValue(\"clockText\", \"backCol\", \"_dpdoesnotexist\");
  } 
  // no selection, so no trigger mechanism needed in this panel.   
}

updateDigBoardCtrl(string dp1, bool connected,
                 string dp2, int clock, 
                 string dp3, bool invalid)
{
  LOG_DEBUG(\"DigBoardCtrl.pnl:updateDigBrdCtrl|entered with connected: \"+ connected + \" clock: \" + clock);  
  if (!invalid) {
    setValue(\"clockText\", \"text\", clock);
    if (enabled) {
      setValue(\"connectedText\", \"backCol\", \"green\");
    } else {
      setValue(\"connectedText\", \"backCol\", \"red\");
    }
  } else {
    setValue(\"connectedText\", \"backCol\", \"Lofar_invalid\");
    setValue(\"clockText\", \"backCol\", \"Lofar_invalid\");
  }  
}

void prepareProcesses() {
  dyn_string result;
  
  dynAppend(result,\",DigBoardCtrl,\"+baseDP);    
  dynAppend(g_processesList,baseDP);
  
  
  // write result to the db so various panels can reset themselves  
  dpSet(DPNAME_NAVIGATOR + g_navigatorID + \".processesList\",result);
  
  LOG_DEBUG(\"DigBoardCtrl.pnl:prepareProcesses|processList found: \"+ result);    
  
}
