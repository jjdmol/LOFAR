    dpConnect(\"fw_alertsEvent\",false,DPNAME_NAVIGATOR + g_navigatorID + \".fw_alerts.event\");
  } else {
		setValue(fw_alerts,\"backCol\",\"_dpdoesnotexist\");
  } 


  LOG_TRACE(\"navigator.pnl:Initialize|end\");  
}


