#!/bin/sh
psql -h dop50 -U postgres test <<_EOF_ > tA_tmp.psql_out 2>&1
\i ../../../test/ABC.sql
\q
_EOF_

# $@ to add a command (like valgrind) to the executable.
# the command has to be supplied via the call to this .run script
# (eg  source tA.run or source tA.run valgrind -log=test.log)
$@ ./tA
