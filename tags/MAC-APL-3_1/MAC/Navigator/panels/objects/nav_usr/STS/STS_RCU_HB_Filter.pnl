"main()
{

  if(dpAccessable($datapoint + \".filSel0\") && 
     dpAccessable($datapoint + \".filSel1\"))
	{
    dpConnect(\"ControlHBFilter\", $datapoint + \".filSel0:_online.._value\",
                                 $datapoint + \".filSel1:_online.._value\",
                                 $datapoint + \".filSel1:_online.._invalid\");
  }
  else
  {
    setValue(\"rect_filter\", \"backCol\", \"_dpdoesnotexist\");
  }
}


ControlHBFilter(string dp1, bool filSel0,
                string dp2, bool filSel1,
                string dp3, bool invalid)
{

  if(!invalid)
  {
    ComboBox1.enabled = TRUE;
    if (!filSel1 && !filSel0)
    {
      ComboBox1.selectedPos=1;    //110-190MHz
      setValue(\"rect_filter\", \"backCol\", \"Lofar_device_active\");
    }
    else if (!filSel1 && filSel0)
    {
      ComboBox1.selectedPos=2;    //170-230MHz
      setValue(\"rect_filter\", \"backCol\", \"Lofar_device_active\");
    }
    else if (filSel1 && !filSel0)
    {
      ComboBox1.selectedPos=3;    //210-250MHz
      setValue(\"rect_filter\", \"backCol\", \"Lofar_device_active\");
    }
    else if (filSel1 && filSel0)
    {
      ComboBox1.selectedPos=4;    //Filter OFF
      setValue(\"rect_filter\", \"backCol\", \"grey\");
    }
  }
  else
  {
    ComboBox1.enabled = FALSE;
    setValue(\"rect_filter\", \"backCol\", \"_dpdoesnotexist\");
  }
