"main()
{

  ////////////////////////////////////////////////////////
  //FILSEL_0   FILSEL_1   Filter     [Date: July 27  2005]
  //   0          0       110-190MHz
  //   0          1       170-230MHz
  //   1          0       210-240MHz
  //   1          1       filter off
  ////////////////////////////////////////////////////////
  //DebugN(\"ComboBoxHB.selectedPos:\"+ComboBoxHB.selectedPos);
  if (ComboBoxHB.selectedPos==1)          //110-190MHz
  {
    dpSet($datapoint + \".filSel1\", FALSE);
    dpSet($datapoint + \".filSel0\", FALSE);
  }
  else if (ComboBoxHB.selectedPos==2)     //170-230MHz
  {
    dpSet($datapoint + \".filSel1\", FALSE);
    dpSet($datapoint + \".filSel0\", TRUE);
  }
  else if (ComboBoxHB.selectedPos==3)     //210-240MHz
  {
    dpSet($datapoint + \".filSel1\", TRUE);
    dpSet($datapoint + \".filSel0\", FALSE);
  }
  else if (ComboBoxHB.selectedPos==4)     //Filter OFF
  {
    dpSet($datapoint + \".filSel1\", TRUE);
    dpSet($datapoint + \".filSel0\", TRUE);
  }

