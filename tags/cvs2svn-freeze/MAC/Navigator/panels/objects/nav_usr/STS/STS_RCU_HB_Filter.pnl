"//
//RCU_HB_Filter (JjS)

main()
{

	// This version shows only the status
  // no control setting is possible
  // so the filtervalue will be shown via a txtfield.
  // If control is possible TXT should be FALSE and Box should be TRUE
	setValue(\"ComboBoxHB\"       , \"visible\", FALSE);
	setValue(\"filterSettingTxt\" , \"visible\", TRUE);

  if(dpAccessable($datapoint + \".filSel0\") && 
     dpAccessable($datapoint + \".filSel1\"))
	{
    dpConnect(\"ControlHBFilter\", $datapoint + \".filSel0:_online.._value\",
                                 $datapoint + \".filSel0:_online.._invalid\",
                                 $datapoint + \".filSel1:_online.._value\",
                                 $datapoint + \".filSel1:_online.._invalid\",
	  														 $datapoint + \".bandSelLBA_HBA:_online.._value\",
	                               $datapoint + \".bandSelLBA_HBA:_online.._invalid\");
  }
  else
  {
    setValue(\"rect_filterHB\", \"backCol\", \"_dpdoesnotexist\");
  }
}


ControlHBFilter(string dp1, bool filSel0,
                string dp2, bool invalid0,
                string dp3, bool filSel1,
                string dp4, bool invalid1,
                string dp5, bool notLBL,
                string dp6, bool bandSel_invalid)
{

  if(!invalid0 && !invalid1 && !bandSel_invalid)
  {
  	if(notLBL)
  	{ // ComboBoxHB.enabled = TRUE;
			
    	rect_filterHB.enabled = TRUE;
    	if (!filSel1 && !filSel0)
    	{
      	//ComboBoxHB.selectedPos=1;    //110-190MHz
      	setValue(\"rect_filterHB\", \"backCol\", \"Lofar_operational\");
      	setValue(\"filterSettingTxt\" , \"text\" , \"110-190 MHz\" );
    	}
    	else if (!filSel1 && filSel0)
    	{
      	//ComboBoxHB.selectedPos=2;    //170-230MHz
      	setValue(\"rect_filterHB\", \"backCol\", \"Lofar_operational\");
      	setValue(\"filterSettingTxt\" , \"text\" , \"170-230 MHz\" );
    	}
    	else if (filSel1 && !filSel0)
    	{
      	//ComboBoxHB.selectedPos=3;    //210-240MHz
      	setValue(\"rect_filterHB\", \"backCol\", \"Lofar_operational\");
      	setValue(\"filterSettingTxt\" , \"text\" , \"210-240 MHz\" );
    	}
    	else if (filSel1 && filSel0)
    	{
      	//ComboBoxHB.selectedPos=4;    //Filter OFF
      	setValue(\"rect_filterHB\", \"backCol\", \"Lofar_bad\");
      	setValue(\"filterSettingTxt\" , \"text\" , \"Filter OFF\" );
    	}
    }
    else
    {
    	setValue(\"rect_filterHB\", \"backCol\", \"Lofar_off\");
    }    
  }
  else
  {
		//ComboBoxHB.enabled = FALSE;
    rect_filterHB.enabled = FALSE;
    setValue(\"rect_filterHB\", \"backCol\", \"Lofar_invalid\");
  }
