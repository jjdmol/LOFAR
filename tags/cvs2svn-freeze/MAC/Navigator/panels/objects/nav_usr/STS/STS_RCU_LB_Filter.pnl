"//
//RCU_LB_Filter (JjS)


main()
{

	// This version shows only the status
  // no control setting is possible
  // so the filtervalue will be shown via a txtfield.
  // If control is possible TXT should be FALSE and Box should be TRUE
	setValue(\"ComboBoxLB\"       , \"visible\", FALSE);
	setValue(\"filterSettingTxt\" , \"visible\", TRUE);

  if(dpAccessable($datapoint + \".filSel0\") && 
     dpAccessable($datapoint + \".filSel1\"))
	{
    dpConnect(\"ControlLBFilter\", $datapoint + \".filSel0:_online.._value\",
                                 $datapoint + \".filSel0:_online.._invalid\",
                                 $datapoint + \".filSel1:_online.._value\",
                                 $datapoint + \".filSel1:_online.._invalid\",
	  														 $datapoint + \".bandSelLBA_HBA:_online.._value\",
	                               $datapoint + \".bandSelLBA_HBA:_online.._invalid\");
  }
  else
  {
    setValue(\"rect_filterLB\", \"backCol\", \"_dpdoesnotexist\");
  }
}


ControlLBFilter(string dp1, bool filSel0,
                string dp2, bool invalid0,
                string dp3, bool filSel1,
                string dp4, bool invalid1,
                string dp5, bool notLBL,
                string dp6, bool bandSel_invalid                )
{

  if(!invalid0 && !invalid1 && !bandSel_invalid)
  {
  	if(!notLBL)
		{
			ComboBoxLB.enabled = TRUE;
    	rect_filterLB.enabled = TRUE;
    	if (!filSel1 && !filSel0)
    	{
      	ComboBoxLB.selectedPos=1;    //10-80MHz
      	setValue(\"rect_filterLB\"    , \"backCol\" , \"Lofar_operational\");
      	setValue(\"filterSettingTxt\" , \"text\"    , \"10-80 MHz\" );
    	}
    	else if (!filSel1 && filSel0)
    	{
      	ComboBoxLB.selectedPos=2;    //30-80MHz
      	setValue(\"rect_filterLB\", \"backCol\", \"Lofar_operational\");
      	setValue(\"filterSettingTxt\" , \"text\" , \"30-80 MHz\" );
    	}
    	else if (filSel1 && !filSel0)
    	{
      	ComboBoxLB.selectedPos=3;    //Filter OFF
      	setValue(\"rect_filterLB\", \"backCol\", \"Lofar_broken\");
      	setValue(\"filterSettingTxt\" , \"text\" , \"Filter OFF\" );
    	}
    	else if (filSel1 && filSel0)
    	{
      	ComboBoxLB.selectedPos=3;    //Filter OFF
      	setValue(\"rect_filterLB\", \"backCol\", \"Lofar_broken\");
      	setValue(\"filterSettingTxt\" , \"text\" , \"Filter OFF\" );
    	}
    }
    else
    {
    	setValue(\"rect_filterLB\", \"backCol\", \"Lofar_off\");
    }
  }
  else
  {
		ComboBoxLB.enabled = FALSE;
    rect_filterLB.enabled = FALSE;
    setValue(\"rect_filterLB\", \"backCol\", \"Lofar_invalid\");
  }
