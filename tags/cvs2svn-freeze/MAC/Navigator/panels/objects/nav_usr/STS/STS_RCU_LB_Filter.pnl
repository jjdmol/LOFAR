"main()
{

  ////////////////////////////////////////////////////////
  //FILSEL_0   FILSEL_1   Filter     [Date: July 27  2005]
  //   0          0         10-80MHz
  //   0          1         30-80MHz
  //   1          0       filter off
  //   1          1       filter off
  ////////////////////////////////////////////////////////
  //DebugN(\"ComboBoxLB.selectedPos:\"+ComboBoxLB.selectedPos);
  if (ComboBoxLB.selectedPos==1)          //10-80MHz
  {
    dpSet($datapoint + \".filSel1\", FALSE);
    dpSet($datapoint + \".filSel0\", FALSE);
  }
  else if (ComboBoxLB.selectedPos==2)     //30-80MHz
  {
    dpSet($datapoint + \".filSel1\", FALSE);
    dpSet($datapoint + \".filSel0\", TRUE);
  }
  else if (ComboBoxLB.selectedPos==3)     //Filter OFF
  {
    dpSet($datapoint + \".filSel1\", TRUE);
    dpSet($datapoint + \".filSel0\", TRUE);
  }

