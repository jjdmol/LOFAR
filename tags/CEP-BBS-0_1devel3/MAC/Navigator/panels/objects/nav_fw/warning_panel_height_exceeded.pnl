            string dp5, int Area1nrOfRows,
            string dp6, int Area2nrOfRows,
            string dp7, int Area3nrOfRows,
            string dp8, int nrOfRowsList)
{
  int label=20, space=10;
  int panelHeigth = 746;
  ListActive =1; //AlertList is always visible
  if (panelHeigth < ( space + label + (space+label+Area1nrOfRows*26)*Area1Active +
                    (space+label+Area2nrOfRows*26)*Area2Active + (space+label+Area3nrOfRows*26)*Area3Active)+
                    (space+label+nrOfRowsList*26)*ListActive)
  {
    setValue (\"txt_info\", \"visible\", TRUE);
    setValue (\"icon_info\", \"visible\", TRUE);
    setValue (\"txt_info_2\", \"visible\", TRUE);
    setValue (\"Border\", \"visible\", TRUE);
    setValue (\"Border3\", \"visible\", TRUE);
  }
  else
  {
    setValue (\"txt_info\", \"visible\", FALSE);
    setValue (\"icon_info\", \"visible\", FALSE);
    setValue (\"txt_info_2\", \"visible\", FALSE);
    setValue (\"Border\", \"visible\", FALSE);
    setValue (\"Border3\", \"visible\", FALSE);
  }
}

