"//Open a panel with two trees.
//Allow to open a such panel only once.
main(){
	string panelName = \"Test with 2 trees\";

	if(!isPanelOpen(panelName)){
		//the panel is not yet opened: we can open it

		ChildPanelOnCentral(\"fwTreeView/fwTreeViewTest2.pnl\", panelName, makeDynString());

		// release ressouces used by fwTreeView.pnl if tree/fwTreeViewTest2.pnl
		//is closed without fwTreeView_close() call:
		fwTreeView_watchDog(panelName);
		                     
	}
}" 0
