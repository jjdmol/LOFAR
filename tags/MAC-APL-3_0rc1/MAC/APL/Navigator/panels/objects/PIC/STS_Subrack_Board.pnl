E"main(int button, bool state)
{
  if (button == 0)
  {
    setValue(\"label_voltage15\", \"visible\", state);
    setValue(\"label_voltage33\", \"visible\", state);
    setValue(\"txt_voltage15\"  , \"visible\", state);
    setValue(\"txt_voltage33\"  , \"visible\", state);
    setValue(\"txt_unitVoltage15\"  , \"visible\", state);
    setValue(\"txt_unitVoltage33\"  , \"visible\", state);

    setValue(\"rect_voltage15\"  , \"visible\", state);
    setValue(\"rect_voltage33\"  , \"visible\", state);
    if (state == TRUE)        // if checkbox is checked, show details and connect textbox to the datapoints
    {
      if(!dpAccessable($datapoint + \"_Board0.voltage15:_online.._value\") ||
         !dpAccessable($datapoint + \"_Board0.voltage33:_online.._value\"))
      {
        setValue(\"rect_voltage15\", \"backCol\", \"_dpdoesnotexist\");
        setValue(\"rect_voltage33\", \"backCol\", \"_dpdoesnotexist\");
        setValue(\"txt_voltage15\", \"visible\", FALSE);
        setValue(\"txt_voltage33\", \"visible\", FALSE);
        return;
