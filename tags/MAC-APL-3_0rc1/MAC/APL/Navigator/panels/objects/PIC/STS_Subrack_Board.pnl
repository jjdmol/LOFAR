    setValue(\"txt_voltage33\"   , \"text\", voltage33);
  }
  else
  {
    setValue(\"txt_voltage15\"   , \"visible\", !invalid);
    setValue(\"txt_voltage33\"   , \"visible\", !invalid);
  }
