#!/bin/sh
# This script writes predicted data. 

if [ "$#" != "1" ]; then
 echo "run as:   run.WritePredData usernm "
 exit 1
fi

usernm=$1

export PATH=../../src:$PATH
clearBB

time src/BBS3 ../../test/tWritePredData.input $usernm
#time src/BBS3 ../../test/tWritePredData.input.test3 $usernm >& run3.log
#time src/BBS3 ../../test/tWritePredData.input.test10 $usernm >& run10.log
#time src/BBS3 ../../test/tWritePredData.input.test84 $usernm >& run84.log

