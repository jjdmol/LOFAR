/////////////////////////////////////////////////////////////////////
//
// Function: set a specific Trend graphical object to a certain state
// FunctionName: setTrendVisible
//
/////////////////////////////////////////////////////////////////////
void setTrend1Properties()
{
  float Trend1Height, Trend1Width, PanelWidth, PanelHeight;
  int Trend1_x, Trend1_y;

  panelSize(\"\", PanelWidth, PanelHeight);
  Trend1Width = 941;
  Trend1Height = 351;
  getValue(\"Trend1\", \"position\", Trend1_x, Trend1_y);
  setValue(\"Trend1\", \"scale\", (PanelWidth-8 - Trend1_x)/(Trend1Width), (PanelHeight-5 - Trend1_y)/(Trend1Height));
  // PanelWidth was voorheen: (PanelWidth-5)
}


/////////////////////////////////////////////////////////////////////
//
// Function: set a specific Trend graphical object to a certain state
// FunctionName: setTrendVisible
//
/////////////////////////////////////////////////////////////////////
void setTrendVisible(int TrendNumber, bool Visible)
{
  setValue(\"Trend\" + TrendNumber + \"_btn_LegendVisible\", \"visible\", Visible);
  setValue(\"Trend\" + TrendNumber + \"_txt_LegendVisible\", \"visible\", Visible);
  setValue(\"Trend\" + TrendNumber + \"_btn_ZoomVisible\", \"visible\", Visible); 
  setValue(\"Trend\" + TrendNumber + \"_txt_ZoomVisible\", \"visible\", Visible);
  setValue(\"Trend\" + TrendNumber, \"visible\", Visible);
  setValue(\"Trend\" + TrendNumber + \"_txt_LabelTitle\", \"visible\", Visible);
  setValue(\"Trend\" + TrendNumber + \"_txt_Title\", \"visible\", Visible);
}


/////////////////////////////////////////////////////////////////////
//
// Function: To plot a single trace in a Trend-plot
// FunctionName: TracePlot
//
/////////////////////////////////////////////////////////////////////
void TracePlot (string dp1, string dpName, string dp2, bool AutoScale,
								string dp3, int ScaleMin,  string dp4, int ScaleMax,
								string dp5, string LegendName)
{
	int TraceNumber, TrendNumber;
  //Check for a filled in dpName, if present start configuring trace-plot
  getTrendNumber(dp1, TrendNumber);
  getTraceNumber(dp1, TraceNumber);
  
  if ((dpName ==\"\") || ($datapoint==\"\"))        //No datapoint configured for this Trace, or no selected item in tree
  {
    setValue(\"Trend\"+TrendNumber, \"curveVisible\", \"curve\"+TraceNumber, FALSE);
    setValue(\"Trend\"+TrendNumber, \"curveScaleVisibility\", \"curve\"+TraceNumber, FALSE);
  }
  else
	{
	  if (dpGetElementName($datapoint)!=\"\")   //SelectedItem is a datapoint elemeent
	  {
      setValue(\"Trend\"+TrendNumber,\"connectDirectly\", \"curve\"+TraceNumber, $datapoint + \":_online.._value\");
	  }
	  else
	  {
		  setValue(\"Trend\"+TrendNumber,\"connectDirectly\", \"curve\"+TraceNumber, $datapoint + \"_\" + dpName + \":_online.._value\");
//		  DebugN(\"dpName: \"+ dpName + \" |$datapoint: \"+ $datapoint);
      setValue(\"Trend\"+TrendNumber,\"curveLegendName\", \"curve\"+TraceNumber, LegendName);
      setValue(\"Trend\"+TrendNumber,\"curveVisible\", \"curve\"+TraceNumber, TRUE);
      setValue(\"Trend\"+TrendNumber,\"curveScaleVisibility\", \"curve\"+TraceNumber, TRUE);
      setValue(\"Trend\"+TrendNumber, \"curveAutoscale\", \"curve\"+TraceNumber, AutoScale);
    }
    if (AutoScale==FALSE)
	  {
      setValue(\"Trend\"+TrendNumber, \"curveMin\", \"curve\"+TraceNumber, ScaleMin);
      setValue(\"Trend\"+TrendNumber, \"curveMax\", \"curve\"+TraceNumber, ScaleMax);
	  }
  }
}


/////////////////////////////////////////////////////////////////////
//
// Function: Function te generate random values for simulation
// FunctionName: GetRandomValue
//
/////////////////////////////////////////////////////////////////////
float GetRandomValue( float fOffset, float fSwing)
{
  float fRandom;
  float fValue;
  
  fRandom = rand();
  fRandom = fRandom / 32767.0;
  fRandom *= fSwing;
  fRandom -= (fSwing / 2.0);
  fValue = fOffset + fRandom;
  return fValue;
}


