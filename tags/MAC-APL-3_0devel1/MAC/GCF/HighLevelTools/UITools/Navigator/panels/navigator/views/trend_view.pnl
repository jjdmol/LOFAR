//////////////////////////////////////////////////////////////////////////////////
//
// FunctionName: CheckTimeInterval
// Function    : Check the time interval for a tred
// 
///////////////////////////////////////////////////////////////////////////////////
CheckTimeInterval(int Days, int Hours, int Minutes, int Seconds, string TrendNumber)
{
  if ((0==Days) && (0==Hours) && (0==Minutes) && (Seconds<2)) 
    dpSet($configDatapoint + \".\"+TrendNumber+\".Minutes\", 5);
    dpSet($configDatapoint + \".\"+TrendNumber+\".Seconds\", 2);
}


//////////////////////////////////////////////////////////////////////////////////
//
// FunctionName: ArrangeTrendPlots
// Function    : Arrange the trend-plots in the panel
// 
///////////////////////////////////////////////////////////////////////////////////
ArrangeTrendPlots(string dp1, bool StandardTrend, string dp2, bool Trend2Active, string dp3, bool DedicatedTrend)
{
	string Trend1Title, Trend2Title;
  //Standard Trend-plot on dpElement
  if ((dpGetElementName($datapoint)!=\"\") && StandardTrend)
  {
    dpSet(\"__nav_SingleElement_Trend.1.Trace1.dpName\", dpSubStr($datapoint, DPSUB_DP_EL_CONF_DET_ATT));
    dpGet(\"__nav_SingleElement_Trend.1.Title\", Trend1Title);
    setValue(\"Trend1_txt_Title\", \"text\", Trend1Title);
		dpConnectElement2Trend();
 	  setTrend1Properties();
		setTrendVisible(1, TRUE);
		setTrendVisible(2, FALSE);
  }
  //Standard Trend-plot on dpType
  else if ((dpGetElementName($datapoint)==\"\") && StandardTrend)
  {
    if (Trend2Active)
    {
//	    DebugN(\"Trend2-Active\");
	    dpGet($configDatapoint + \".1.Title\", Trend1Title);
 	    dpGet($configDatapoint + \".2.Title\", Trend2Title);
	    setValue(\"Trend1_txt_Title\", \"text\", Trend1Title);
 	    setValue(\"Trend2_txt_Title\", \"text\", Trend2Title);
      setValue(\"Trend1\", \"scale\", 1, 1);
			dpConnectTrend(1,1);			//Trend1, Trace1
			dpConnectTrend(1,2);
			dpConnectTrend(1,3);
			dpConnectTrend(1,4);
			dpConnectTrend(1,5);
			dpConnectTrend(1,6);
			dpConnectTrend(1,7);
			dpConnectTrend(1,8);
 			dpConnectTrend(2,1);			//Trend2, Trace1
			dpConnectTrend(2,2);
			dpConnectTrend(2,3);
			dpConnectTrend(2,4);
			dpConnectTrend(2,5);
			dpConnectTrend(2,6);
			dpConnectTrend(2,7);
			dpConnectTrend(2,8);
			setTrendVisible(1, TRUE);
			setTrendVisible(2, TRUE);
      dpConnect(\"CalculateTimeInterval\", $configDatapoint + \".1.Days\",
                                         $configDatapoint + \".1.Hours\",
                                         $configDatapoint + \".1.Minutes\",
                                         $configDatapoint + \".1.Seconds\");

      dpConnect(\"CalculateTimeInterval\", $configDatapoint + \".2.Days\",
                                         $configDatapoint + \".2.Hours\",
                                         $configDatapoint + \".2.Minutes\",
                                         $configDatapoint + \".2.Seconds\");
    }
    else
    {	
 	    dpGet($configDatapoint + \".1.Title\", Trend1Title);
	    setValue(\"Trend1_txt_Title\", \"text\", Trend1Title);
			dpConnectTrend(1,1);			//Trend1, Trace1
			dpConnectTrend(1,2);
			dpConnectTrend(1,3);
			dpConnectTrend(1,4);
			dpConnectTrend(1,5);
			dpConnectTrend(1,6);
			dpConnectTrend(1,7);
			dpConnectTrend(1,8);
			setTrendVisible(1, TRUE);
			setTrend1Properties();     //retrieve sizes and scale to fit
		  setTrendVisible(2, FALSE);
      dpConnect(\"CalculateTimeInterval\", $configDatapoint + \".1.Days\",
                                         $configDatapoint + \".1.Hours\",
                                         $configDatapoint + \".1.Minutes\",
                                         $configDatapoint + \".1.Seconds\");
    }
  }
  //Dedicated Trend-plot panel
  else if ((!StandardTrend) && (DedicatedTrend))
  {
    string DedicatedPanelName;
    dpGet($configDatapoint + \".DedicatedPanelName\", DedicatedPanelName);
    DebugN(\"Load dedicated_trend.panel: \" + DedicatedPanelName);    
  }
} 

/////////////////////////////////////////////////////////////////////
//
// Function: Calculates the timeinterval to display in the trend display
// FunctionName: CalculateTimeInterval
//
/////////////////////////////////////////////////////////////////////
CalculateTimeInterval(string dp1, int Days, string dp2, int Hours,
                      string dp3, int Minutes, string dp4, int Seconds)
{
  int TrendNumber;
  time interval, verschil, starttime, end;

  getTrendNumber(dp1, TrendNumber);
  getValue(\"Trend\"+TrendNumber, \"visibleTimeRange\", 0, starttime, end);
  interval=makeTime(year(starttime), month(starttime), day(starttime), hour(starttime)+Hours, minute(starttime)+Minutes, second(starttime)+Seconds);
  setValue(\"Trend1\", \"trendStop\");
  setValue(\"Trend1\", \"timeInterval\", interval-starttime);
  setValue(\"Trend1\", \"trendRun\");
}


/////////////////////////////////////////////////////////////////////
//
// Function: connect element to trend
// FunctionName: dpConnectTrend
//
/////////////////////////////////////////////////////////////////////
void dpConnectTrend(int TrendNumber, int TraceNumber)
{
	dpConnect(\"TracePlot\", $configDatapoint + \".\" + TrendNumber + \".Trace\" + TraceNumber + \".dpName\",
												 $configDatapoint + \".\" + TrendNumber + \".Trace\" + TraceNumber + \".AutoScale\",
												 $configDatapoint + \".\" + TrendNumber + \".Trace\" + TraceNumber + \".ScaleMin\",
												 $configDatapoint + \".\" + TrendNumber + \".Trace\" + TraceNumber + \".ScaleMax\",
												 $configDatapoint + \".\" + TrendNumber + \".Trace\" + TraceNumber + \".LegendName\");
}


